      )\n            ) : null}\n            <h5 className=\"header-nav-grid__title text-sm font-bold mb-2.5\">\n              <Button\n                className=\"transition-opacity duration-300 ease-in-out hover:opacity-80\"\n                href={subItem.link.url}\n              >\n                {subItem.link.title}\n              </Button>\n            </h5>\n            {subItem.children?.length ? (\n              <ul className=\"header-nav-grid-list space-y-1\">\n                {subItem.children.map((item: any, thirdIndex: number) => (\n                  <li\n                    className=\"header-nav-grid-list__item\"\n                    key={`${item.link.title}-${thirdIndex}`}\n                  >\n                    <Button\n                      href={item.link.url}\n                      target={item.link.target}\n                      className=\"header-nav-grid-list__btn text-sm transition-opacity duration-300 ease-in-out hover:opacity-80\"\n                    >\n                      {item.link.title}\n                    </Button>\n                  </li>\n                ))}\n              </ul>\n            ) : null}\n          </article>\n        ))}\n      </div>\n    </div>\n  )\n}\n\nexport default ThirdLevelGrid\n","import React, { useContext, useState } from 'react'\nimport { HeaderContext } from '../..'\nimport cn from 'classnames'\n\nimport Button from '../../../button'\nimport { HeaderNavItem } from '../types'\nimport SecondLevel from './second-level'\nimport ThirdLevel from './third-level'\nimport ThirdLevelGrid from './third-level-grid'\n\nconst NavComponents: any = {\n  secondLevel: SecondLevel,\n  thirdLevel: ThirdLevel,\n  thirdLevelGrid: ThirdLevelGrid,\n}\n\nconst DeskMenuItem: React.FC<HeaderNavItem> = ({ data, index }) => {\n  const {\n    setActiveMask,\n    PlatformComponents,\n    controls: {\n      general: {\n        effects: { hoverBorderColor, hoverBold, hoverOpacity },\n      },\n    },\n  } = useContext(HeaderContext)\n  const [activeSubContent, setActiveSubContent] = useState(false)\n\n  function toggleContent(state: boolean) {\n    if (data.type && data.type !== 'firstLevel') {\n      setActiveMask(state)\n    }\n    setActiveSubContent(state)\n  }\n\n  return (\n    <li\n      aria-hidden=\"true\"\n      className={cn(\n        'header-nav__item text-[color:var(--dark)] mobile-only:hidden',\n        {\n          relative: data.type === 'secondLevel',\n        }\n      )}\n      onClick={() => toggleContent(false)}\n      onMouseEnter={() => toggleContent(true)}\n      onMouseLeave={() => toggleContent(false)}\n    >\n      <Button\n        className={cn(\n          'block text-sm py-2 px-3.5 duration-300 relative text-white',\n          {\n            'pl-0': index === 0,\n            'font-semibold': activeSubContent && hoverBold,\n            'opacity-60': activeSubContent && hoverOpacity,\n          }\n        )}\n        href={data.link.url}\n        PlatformComponents={PlatformComponents}\n        target={data.link.target}\n      >\n        {data.link.title}\n        <div\n          className={cn(\n            'header-nav__line h-[2px] -translate-x-1/2 left-1/2 absolute bottom-0 duration-300',\n            {\n              'w-0': !activeSubContent || !hoverBorderColor,\n              'w-full': activeSubContent && hoverBorderColor,\n            }\n          )}\n          style={{\n            backgroundColor: hoverBorderColor,\n          }}\n        />\n      </Button>\n\n      {data.showBanner && data.type === 'firstLevel' ? (\n        <div\n          className={\n            activeSubContent\n              ? 'header-nav__sub header-nav-box-banner active'\n              : 'header-nav__sub header-nav-box-banner'\n          }\n        >\n          <Button\n            className=\"header-nav-box-banner__btn\"\n            href={data.link.url}\n            style={{ maxWidth: `${data.maxWidth}px` }}\n            target={data.link.target}\n          >\n            <img\n      