 </h5>\n          )}\n\n          <ul\n            className={cn('footer-nav__items', {\n              'px-4 md:px-0 flex items-center flex-wrap':\n                column.type === 'images',\n              'mobile-only:pb-6': column.type === 'images',\n              'mobile-only:px-8 mobile-only:max-h-0 mobile-only:overflow-hidden mobile-only:transition-all mobile-only:duration-300':\n                column.type === 'links',\n            })}\n          >\n            {column.type === 'links' && column.links?.length\n              ? column.links.map((link: any, linkIndex: any) => (\n                  <li\n                    className=\"footer-nav__item font-light mb-5 last-of-type:mb-0 text-sm\"\n                    key={`item-${linkIndex}`}\n                  >\n                    <ButtonLink\n                      href={FormatsUtils.link(link.link.url)}\n                      target={link.link.target && link.link.target}\n                      className=\"footer-nav__btn\"\n                      key={`item-link-${linkIndex}`}\n                      PlatformComponents={PlatformComponents}\n                    >\n                      {link.link.title}\n                    </ButtonLink>\n                  </li>\n                ))\n              : null}\n\n            {column.type === 'images' && column.images?.length\n              ? column.images.map((image: any, imageIndex: any) =>\n                  image.link ? (\n                    <li\n                      className=\"footer-nav__item mt-0 mr-2.5 mb-2.5 ml-0\"\n                      key={`item-image-${imageIndex}`}\n                    >\n                      <ButtonLink\n                        className=\"footer-nav__btn\"\n                        href={image.link.url}\n                        target={image.link.target && image.link.target}\n                        key={`item-link-${imageIndex}`}\n                        PlatformComponents={PlatformComponents}\n                      >\n                        <LazyImage\n                          image={image.image}\n                          className={'footer-nav__img max-w-[50px]'}\n                        />\n                      </ButtonLink>\n                    </li>\n                  ) : (\n                    <li\n                      className=\"footer-nav__item mt-0 mr-2.5 mb-2.5 ml-0\"\n                      key={`item-image-${imageIndex}`}\n                    >\n                      <LazyImage\n                        image={image.image}\n                        className={'footer-nav__img max-w-[50px]'}\n                      />\n                    </li>\n                  )\n                )\n              : null}\n          </ul>\n        </div>\n      )\n    })\n  }\n\n  function renderRow(data: any, variation: string, rowStyle: Style) {\n    if (variation !== 'stores-index' && Object.keys(data).length === 0) {\n      return null\n    }\n\n    return (\n      <div\n        className={cn('footer__row mobile-only:mb-6 lg:py-8', {\n          'mobile-only:border-b mobile-only:border-light mobile-only:pb-6 mobile-only:mb-0':\n            variation === 'stores-index',\n          'mobile-only:mb-0': variation === 'second-section',\n          'lg:border-t lg:border-light': variation !== 'first-section',\n        })}\n        style={{\n          ...rowStyle,\n          borderColor: borderColor,\n        }}\n      >\n        <div className=\"container mobile-only:p-0\">\n          <div className=\"footer__wrapper md:flex justify-between\">\n            {variation === 'stores-index' ? (\n              <StoresIndex\n                filterType={filterType}\n                groupFilter={groupFilter}\n                PlatformComponents={PlatformComponents}\n                stateCategories={stateCategories}\n                style={{\n                  filterBg,\n                  color,\n                }}\n                title={storesTitle}\n              />\n            ) : (\n              renderColumns(data, variation)\n            )}\n          </div>\n        </div>\n      </div>\n    )\n  }\n\n  useEffect(() => {\n    if (\n      window?.location.href.search('nossas-lojas') < 0 &&\n      !!showStoresFilter\n    ) {\n      setShowFilter(true)\n    } else {\n      setShowFilter(false)\n    }\n  }, [showStoresFilter])\n\n  return (\n    <div\n      className=\"footer relative\"\n      style={{\n        color: color,\n      }}\n    >\n      {row &&\n        renderRow(row, 'first-section', {\n          backgroundColor: firstSectionBg,\n        })}\n\n      {showFilter &&\n        renderRow(null, 'stores-index', {\n          backgroundColor: filterBg,\n        })}\n\n      {secondRow &&\n        renderRow(secondRow, 'second-section', {\n          backgroundColor: secondSectionBg,\n        })}\n\n      {copyright && (\n        <div\n          className=\"copyright md:mt-0 py-6 md:py-8 text-center border-t border-light\"\n          dangerouslySetInnerHTML={{ __html: copyright }}\n          style={{\n            backgroundColor: copyrightBg,\n            borderColor: borderColor,\n          }}\n        />\n      )}\n    </div>\n  )\n}\n\nexport default Footer\n","import React, { useContext, memo } from 'react'\nimport LazyImage from '../../lazy-image'\nimport Button from '../../button'\nimport { HeaderContext } from '..'\n\nconst Profile: React.FC = () => {\n  const {\n    content: {\n      profile: { icon, loggedContent, unLoggedContent },\n    },\n    controls: {\n      style: { userTextColor },\n    },\n    PlatformComponents,\n    PlatformHooks,\n  } = useContext(HeaderContext)\n\n  if (!PlatformHooks?.useSession) return null\n\n  const { person } = PlatformHooks.useSession()\n\n  return (\n    <Button\n      href={person?.id ? '/account' : '/login'}\n      className=\"header-profile flex items-center\"\n      PlatformComponents={PlatformComponents}\n    >\n      {icon ? (\n        <LazyImage\n          image={icon}\n          style={{\n            width: '30px',\n            marginRight: '10px',\n          }}\n        />\n      ) : null}\n      <span\n        className=\"text-[12px] leading-4 md:text-sm\"\n        dangerouslySetInnerHTML={{\n          __html: person?.id ? loggedContent : unLoggedContent,\n        }}\n        style={{ color: userTextColor }}\n      />\n    </Button>\n  )\n}\n\nexport default memo(Profile)\n","import React, { useContext } from 'react';\nimport { BurgerProps } from './types';\nimport { HeaderContext } from '../';\nimport cn from 'classnames'\n\nconst Burger: React.FC<BurgerProps> = ({\n  size,\n  variation,\n  children,\n  visibleDesk = false,\n  color\n}) => {\n  const {\n    toggleNav,\n    controls: {\n      style: {\n        menuBurgerColor\n      }\n    }\n  } = useContext(HeaderContext)\n\n  return (\n    <div className={cn('header-burger', variation, {\n      'md:hidden': !visibleDesk\n    })}>\n      <button\n        className={cn('header-burger-button cursor-pointer w-8', size)}\n        onClick={toggleNav}\n        style={{\n          color: color || menuBurgerColor || 'black'\n        }}\n      >\n        <i className='w-full h-0.5 block mb-1 bg-current' />\n        <i className='w-full h-0.5 block mb-1 bg-current' />\n        <i className='w-9/12 h-0.5 block bg-current' />\n      </button>\n      {children ? children : null}\n    </div>\n  )\n}\n\nexport default Burger;\n","import React, { useContext } from 'react'\nimport LazyImage from '../../lazy-image'\nimport Button from '../../button'\nimport { HeaderContext } from '..'\n\ntype Props = {\n  children?: React.ReactNode | React.ReactNode[]\n}\n\nconst Wishlist: React.FC<Props> = ({ children }) => {\n  const {\n    content: { wishlist },\n    PlatformComponents,\n  } = useContext(HeaderContext)\n\n  return (\n    <Button\n      href=\"/wishlist\"\n      className=\"header-wishlist ml-auto\"\n      PlatformComponents={PlatformComponents}\n    >\n      {wishlist?.icon?.url ? (\n        <LazyImage className=\"w-8 mx-auto\" image={wis