rn (\n      <div className=\"header-nav-banner\">\n        <Button\n          href={link}\n          className=\"header-nav-banner__btn relative block w-full h-full\"\n          PlatformComponents={PlatformComponents}\n        >\n          <div\n            className=\"bg-cover bg-center absolute -top-2.5 -right-2.5 h-[calc(100%+20px)] w-full object-cover\"\n            style={{\n              backgroundImage: `url(${image.url})`,\n            }}\n          />\n          {/* <LazyImage\n            src={image.url}\n            alt={image.alt}\n            width={image.width}\n            height={image.height}\n          /> */}\n        </Button>\n      </div>\n    )\n  }\n\n  function separeteArr(arr: any, size: number) {\n    const newArr: any = []\n    let i = 0\n\n    while (i < arr.length) {\n      newArr.push(arr.slice(i, i + size))\n      i += size\n    }\n\n    return newArr\n  }\n\n  function getMenuLink(menuItem: any) {\n    return menuItem.link.url\n  }\n\n  function handleNavThirdLevel(action: string) {\n    if (!navThirdLevel?.length) return\n\n    if (action === 'enter') {\n      setActiveContent(true)\n    } else {\n      setActiveContent(false)\n    }\n  }\n\n  useEffect(() => {\n    const itemClass = ['header-nav__item']\n\n    if (data.children && data.children.length >= 1) {\n      setSubItemContent(separeteArr(data.children, 9))\n      itemClass.push('header-nav__item--sub-hasitems')\n    }\n\n    if (activeContent) {\n      itemClass.push('active')\n    }\n\n    setClassItem(itemClass)\n    setNavThirdLevel(data.children)\n  }, [activeContent, data.children])\n\n  if (!data) return null\n\n  return (\n    <li\n      className={classItem.join(' ')}\n      onMouseEnter={() => handleNavThirdLevel('enter')}\n      onMouseLeave={() => handleNavThirdLevel('leave')}\n    >\n      <Button\n        href={getMenuLink(data)}\n        className={\n          'header-nav__btn text-sm font-medium relative py-2.5 px-5 pl-[40px] flex items-center justify-between 