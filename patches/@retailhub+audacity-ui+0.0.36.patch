     <div className=\"header-nav__header px-4 absolute top-0 left-0 w-full h-[38px] flex items-center uppercase bg-[color:var(--primary)] text-white\">\n            <Button\n              className=\"header-nav-back__btn flex items-center justify-center w-[26px] h-[26px] bg-white rounded-[3px] before:content-[''] before:block before:w-2 before:h-2 before:border before:border-[color:var(--primary)] before:border-t-0 before:border-r-0 before:relative before:left-0.5 before:rotate-45\"\n              onClick={(_e: any) => toggleContent(false)}\n            />\n            <Button\n              href={data.link.url}\n              className=\"ml-2.5 hover:underline text-sm\"\n              onClick={(_e: any) => toggleContent(false)}\n            >\n              {data.link.title}\n            </Button>\n          </div>\n          <ul className=\"header-nav__items header-nav__items--sub space-y-4\">\n            {data.children.map((subItem: any, subItemIndex: number) => (\n              <SecondLevel\n                data={subItem}\n                key={`second-level-${subItemIndex}`}\n              />\n            ))}\n          </ul>\n        </div>\n      )}\n    </li>\n  )\n}\n\nexport default MobileMenuItem\n","import React, { useContext } from 'react'\nimport { HeaderContext } from '../../..'\nimport cn from 'classnames'\n\nimport Button from '../../../../button'\nimport { HeaderNavSubItem } from '../../types'\n\nconst SecondLevel: React.FC<HeaderNavSubItem> = ({\n  data,\n  activeSubContent,\n  navSubItemsColor,\n}) => {\n  const { PlatformComponents } = useContext(HeaderContext)\n\n  function getMenuLink(menuItem: any) {\n    if (menuItem.children?.length) {\n      return null\n    }\n\n    return menuItem.link.url\n  }\n\n  if (!data?.children?.length) return null\n\n  return (\n    <div\n      className={cn(\n        'header-nav__sub absolute z-10 top-full min-w-full left-0 transition duration-300 ease-in-out bg-white pointer-events-inherit shadow-md',\n       