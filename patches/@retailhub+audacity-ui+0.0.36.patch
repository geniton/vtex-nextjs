    )}\n      style={{\n        color: navSubItemsColor,\n      }}\n    >\n      <ul className=\"header-nav__items header-nav__items--sub\">\n        {data.children.map((subItem: any, subItemIndex: number) => (\n          <li className=\"header-nav__item\" key={`two-level-${subItemIndex}`}>\n            <Button\n              href={getMenuLink(data)}\n              className=\"header-nav__btn block w-full py-2 px-3.5\"\n              PlatformComponents={PlatformComponents}\n            >\n              <span className=\"text-[color:var(--dark)] text-sm\">\n                {subItem.link.title}\n              </span>\n            </Button>\n          </li>\n        ))}\n      </ul>\n    </div>\n  )\n}\n\nexport default SecondLevel\n","import React, { useState, useEffect, useContext } from 'react'\n\nimport Button from '../../../../../button'\nimport { HeaderContext } from '../../../..'\nimport { HeaderNavSubItem } from '../../../types'\nimport cn from 'classnames'\n// import LazyImage from '../../../../../lazy-image'\n\nconst SubItem: React.FC<HeaderNavSubItem> = ({ data, active, index }) => {\n  const { PlatformComponents } = useContext(HeaderContext)\n\n  const [activeContent, setActiveContent] = useState(false)\n  const [classItem, setClassItem] = useState<any>([])\n  const [navThirdLevel, setNavThirdLevel] = useState([])\n  const [subItemContent, setSubItemContent] = useState([])\n\n  useEffect(() => {\n    if (index === 0 && active) {\n      setActiveContent(active)\n    }\n  }, [active, index])\n\n  function navBanner() {\n    const {\n      banner: { image, link },\n    } = data\n\n    return (\n      <div className=\"header-nav-banner\">\n        <Button\n          href={link}\n          className=\"header-nav-banner__btn relative block w-full h-full\"\n          PlatformComponents={PlatformComponents}\n        >\n          <div\n            className=\"bg-cover bg-center absolute -top-2.5 -right-2.5 h-[calc(100%+20px)] w-full object-cover\"\n            style={{\n              backgroundImage: `url(${image.url})`,\n            }}\n          />\n          {/* <LazyImage\n            src={image.url}\n            alt={image.alt}\n            width={image.width}\n            height={image.height}\n          /> */}\n        </Button>\n      </div>\n    )\n  }\n\n  function separeteArr(arr: any, size: number) {\n    const newArr: any = []\n    let i = 0\n\n    while (i < arr.length) {\n      newArr.push(arr.slice(i, i + size))\n      i += size\n    }\n\n    return newArr\n  }\n\n  function getMenuLink(menuItem: any) {\n    return menuItem.link.url\n  }\n\n  function handleNavThirdLevel(action: string) {\n    if (!navThirdLevel?.length) return\n\n    if (action === 'enter') {\n      setActiveContent(true)\n    } else {\n      setActiveContent(false)\n    }\n  }\n\n  useEffect(() => {\n    const itemClass = ['header-nav__item']\n\n    if (data.children && data.children.length >= 1) {\n      setSubItemContent(separeteArr(data.children, 9))\n      itemClass.push('header-nav__item--sub-hasitems')\n    }\n\n    if (activeContent) {\n      itemClass.push('active')\n    }\n\n    setClassItem(itemClass)\n    setNavThirdLevel(data.children)\n  }, [activeContent, data.children])\n\n  if (!data) return null\n\n  return (\n    <li\n      className={classItem.join(' ')}\n      onMouseEnter={() => handleNavThirdLevel('enter')}\n      onMouseLeave={() => handleNavThirdLevel('leave')}\n    >\n      <Button\n        href={getMenuLink(data)}\n        className={\n          'header-nav__btn text-sm font-medium relative py-2.5 px-5 pl-[40px] flex items-center justify-between after:content-[\"\"] after:block after:border-2 after:border-[color:var(--dark)] after:w-2 after:h-2 after:border-l-0 after:border-t-0 after:-rotate-45'\n        }\n        PlatformComponents={PlatformComponents}\n      >\n        <span className=\"relative pb-[5px]\">\n          {data.link.title}\n\n          <span\n            className={cn(\n              'absolute left-0 bottom-0 w-full bg-[color:var(--primary)] transition-all duration-200 ease-in-out hover:h-[2px]',\n              {\n                'h-0': !activeContent,\n                'h-[2px]': activeContent,\n              }\n            )}\n          ></span>\n        </span>\n      </Button>\n      <div\n        className={cn(\n          'header-nav__content absolute h-full top-0 right-0 bg-white w-4/5 p-2.5 pl-[23px]',\n          {\n            'invisible opacity-0': !activeContent,\n            'visible opacity-100': activeContent,\n          }\n        )}\n      >\n        <div\n          className={cn(\n            'header-nav__box grid grid-cols-[60%_40%] transition-all duration-300 ease-in-out h-full',\n            {\n              'animate-[fade-effect_200ms_ease-in-out]': activeContent,\n            }\n          )}\n        >\n          <div className=\"header-nav-list pr-[10px]\">\n            <h3 className=\"header-nav-list__title font-normal mb-3\">\n              {data.link.title}\n            </h3>\n\n            <div className=\"header-nav-list__wrapper flex gap-[23px]\">\n              {subItemContent &&\n                subItemContent.length >= 1 &&\n                subItemContent.map((el: any, subItemIndex: number) => (\n                  <ul\n                    className=\"header-nav-list__items\"\n                    key={`item-subitem-${subItemIndex}`}\n                  >\n                    {el.map((subSubItem: any, subSubItemIndex: number) => (\n                      <li\n                        className=\"header-nav-list__item\"\n                        key={`item-subsubitem-${subSubItemIndex}`}\n                      >\n                        <Button\n                          href={subSubItem.link.url}\n                          className=\"header-nav-list__btn text-sm font-light mb-2 inline-block\"\n                          PlatformComponents={PlatformComponents}\n                        >\n                          {subSubItem.link.title}\n                        </Button>\n                      </li>\n                    ))}\n                  </ul>\n                ))}\n            </div>\n          </div>\n          {data.showBanner ? navBanner() : null}\n        </div>\n      </div>\n    </li>\n  )\n}\n\nexport default SubItem\n","import React from 'react'\n\nimport SubItem from './sub-item'\nimport cn from 'classnames'\nimport { HeaderNavSubItem } from '../../types'\n\nconst ThirdLevel: React.FC<HeaderNavSubItem> = ({\n  data,\n  activeSubContent,\n  navSubItemsColor,\n}) => {\n  if (!data?.children?.length) return null\n\n  return (\n    <div\n      className={cn(\n        'header-nav__sub absolute left-0 top-full w-full min-h-full',\n        `header-nav__sub--${data.type}`,\n        {\n          'invisible opacity-0 pointer-events-none': !activeSubContent,\n          'visible animate-[fade-effect_300ms_ease-in-out]': activeSubContent,\n        }\n      )}\n      style={{\n        color: navSubItemsColor,\n      }}\n    >\n      <ul className=\"header-nav__items header-nav__items--sub bg-light w-1/5 min-h-[430px]\">\n        {data.children.map((subItem: any, subItemIndex: number) => (\n          <SubItem\n            key={`third-level-${subItemIndex}`}\n            index={subItemIndex}\n            active={activeSubContent}\n            data={subItem}\n          />\n        ))}\n      </ul>\n    </div>\n  )\n}\n\nexport default ThirdLevel\n","import React from 'react'\nimport Button from '../../../../button'\nimport cn from 'classnames'\nimport { HeaderNavSubItem } from '../../types'\n\nconst ThirdLevelGrid: React.FC<HeaderNavSubItem> = ({\n  data,\n  activeSubContent,\n  navSubItemsColor,\n}) => {\n  if (!data?.children?.length) return null\n\n  return (\n    <div\n      className={cn(\n        'header-nav__sub absolute top-full w-full left-0 py-5 bg-white px-10',\n        `header-nav__sub--${data.type}`,\n        {\n          'invisible opacity-0 pointer-events-none': !activeSubContent,\n      