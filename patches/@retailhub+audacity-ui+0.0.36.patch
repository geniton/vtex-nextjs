\nconst VerticalNavFirstLevel: React.FC<any> = ({\n  activeNavIndex,\n  data,\n  index,\n  navSubData,\n  onNavSubData,\n  onActiveNavIndex,\n}) => {\n  const {\n    PlatformComponents,\n    toggleActiveVerticalNav,\n    activeVerticalNav,\n  } = useContext(HeaderContext)\n\n  if (navSubData && activeNavIndex !== index) {\n    return null\n  }\n\n  return (\n    <>\n      {data.hasChildren ? (\n        <button\n          className=\"vertical-nav__btn flex items-center text-sm pt-2 pb-2 pl-[22px] border-b border-light w-full\"\n          onClick={() => {\n            onNavSubData(data)\n            onActiveNavIndex(index)\n          }}\n        >\n          {data.showIcon ? (\n            <LazyImage\n              alt={data.icon.alt}\n              className=\"vertical-nav__icon w-5 mr-1\"\n              height={data.icon.height}\n              src={data.icon.url}\n              width={data.icon.width}\n            />\n          ) : null}\n          {data.link?.title}\n        </button>\n      ) : (\n        <Button\n          className=\"vertical-nav__btn text-sm pt-2 pb-2 pl-[22px] border-b border-light w-full\"\n          href={data.link?.url}\n          onClick={() => toggleActiveVerticalNav(!activeVerticalNav)}\n          PlatformComponents={PlatformComponents}\n          target={data.link?.target}\n        >\n          {data.link?.title}\n        </Button>\n      )}\n\n      {navSubData ? (\n        <ul className=\"vertical-nav__items vertical-nav__items--second-level absolute top-0 left-0 bg-white w-full\">\n          {data.children.map((secondlevel: any, dataIndex: number) => (\n            <li\n              className=\"vertical-nav__item\"\n              key={`${secondlevel.link.title}--${dataIndex}`}\n            >\n              <Button\n                className=\"vertical-nav__btn text-sm pt-2 pb-2 pl-[22px] block border-b border-light\"\n                href={secondlevel.link.url}\n                PlatformComponents={PlatformComponents}\n                target={secondlevel.link.target}\n              >\n                {secondlevel.link.title}\n              </Button>\n              {secondlevel.children?.length ? (\n                <ul className=\"vertical-nav__items vertical-nav__items--third-level\">\n                  {secondlevel.children.map(\n                    (thirdlevel: any, secondLevelIndex: number) => (\n                      <li\n                        className=\"vertical-nav__item\"\n                        key={`${thirdlevel.link.title}--${secondLevelIndex}`}\n                      >\n                        <Button\n                          className=\"vertical-nav__btn relative text-sm pt-2 pb-2 pl-[22px] flex items-center border-b border-light before:block before:content-[''] before:w-2 before:h-2 before:border before:border-r-0 before:border-b-0 before:mr-3 before:rotate-[135deg]\"\n                          href={thirdlevel.link.url}\n                          PlatformComponents={PlatformComponents}\n                          target={thirdlevel.link.target}\n                        >\n                          {thirdlevel.link.title}\n                        </Button>\n                      </li>\n                    )\n                  )}\n                </ul>\n              ) : null}\n            </li>\n          ))}\n        </ul>\n      ) : null}\n    </>\n  )\n}\n\nexport default VerticalNavFirstLevel\n","import React, { useContext, useState } from 'react'\nimport VerticalNavFirstLevel from './first-level'\n\nimport Button from '../../button'\nimport { HeaderContext } from '..'\nimport classNames from 'classnames'\n\nconst VerticalNav: React.FC = () => {\n  const {\n    activeVerticalNav,\n    content: {\n      menus: {\n        vertical: { menu, title },\n      },\n    },\n    PlatformComponents,\n    toggleActiveVerticalNav,\n  } = useContext(HeaderContext)\n\n  const [navSubData, setNavSubData] = useState<any>(null)\n  const [activeNavIndex, setActiveNavIndex] = useState(null)\n\n  if (!menu?.length) return null\n\n  return (\n    <nav\n      className={classNames(\n        'vertical-nav w-11/12 max-w-xs h-screen fixed left-0 top-0 z-20 ease-in-out duration-300',\n        {\n          '-translate-x-full opacity-0': !activeVerticalNav,\n          'translate-x-0 opacity-100': activeVerticalNav,\n        }\n      )}\n    >\n      <header className=\"vertical-nav-header flex items-center justify-between border-b border-light bg-light h-16 py-[25px] px-[22px]\">\n        {navSubData ? (\n          <>\n            <button\n              className=\"vertical-nav-header__btn text-sm flex items-center relative before:block before:content-[''] before:w-2 before:h-2 before:border-2 before:border-r-0 before:border-b-0 before:border-l-2 before:mr-3 before:rotate-[-45deg]\"\n              onClick={() => {\n                setNavSubData(null)\n                setActiveNavIndex(null)\n              }}\n            >\n              {navSubData.link?.title}\n            </button>\n            {navSubData.link?.url ? (\n              <Button\n                className=\"vertical-nav-header__close text-[12px] underline\"\n                href={`/${navSubData.link?.url}`}\n                onClick={() => toggleActiveVerticalNav(!activeVerticalNav)}\n                PlatformComponents={PlatformComponents}\n              >\n                Ver tudo\n              </Button>\n            ) : null}\n          </>\n        ) : (\n          <>\n            {title && (\n              <h4 className=\"vertical-nav-header__title max-w-48 text-sm line-clamp-2\">\n                {title}\n              </h4>\n            )}\n            <button\n              className=\"vertical-nav-header__close text-[12px] underline\"\n              onClick={() => toggleActiveVerticalNav(!activeVerticalNav)}\n            >\n              Fechar\n            </button>\n          </>\n        )}\n      </header>\n      <ul className=\"vertical-nav__items vertical-nav__items--first-level h-full bg-white\">\n        {menu.map((firstlevel: any, index: number) => (\n          <li className=\"vertical-nav__item relative\" key={index}>\n            {firstlevel.hasChildren && firstlevel.children?.length ? (\n              <VerticalNavFirstLevel\n                data={firstlevel}\n                navSubData={navSubData}\n                onNavSubData={(data: any) => setNavSubData(data)}\n                index={index + 1}\n                activeNavIndex={activeNavIndex}\n                onActiveNavIndex={setActiveNavIndex}\n              />\n            ) : null}\n          </li>\n        ))}\n      </ul>\n    </nav>\n  )\n}\n\nexport default VerticalNav\n","import React, { useContext } from 'react'\nimport { HeaderContext } from '..'\nimport cn from 'classnames'\n\nconst Mask: React.FC<any> = () => {\n  const { toggleNav, activeNav, activeVerticalNav, activeMask } = useContext(\n    HeaderContext\n  )\n\n  const active = activeNav || activeVerticalNav || activeMask\n  return (\n    <div\n      className={cn(\n        'header-mask fixed z-[10] top-0 left-0 bg-black bg-opacity-60 w-screen h-screen ease duration-300 md:-z-[1]',\n        {\n          'opacity-100 visible': active,\n          'opacity-0 w-100 invisible': !active,\n        }\n      )}\n      onClick={toggleNav}\n      aria-hidden=\"true\"\n    />\n  )\n}\n\nexport default Mask\n","/* eslint-disable react-hooks/exhaustive-deps */\nimport React, { useState, useEffect, createContext } from 'react'\nimport { useScrollYPosition } from 'react-use-scroll-position'\nimport LazyImage from '../lazy-image'\nimport Profile from './profile'\nimport Burger from './burger'\nimport Wishlist from './wishlist'\nimport { HeaderContent, HeaderControls, HeaderProps } from './types'\nimport Nav from './nav'\nimport BottomMobileNav from './bottom-mobile-nav'\nimport VerticalNav from './vertical-nav'\nimport Mask from './mask'\n\n// IT'S VERY IMPORTANT\n// without it the app won't render the components configs properly\n// export app configs json file\nimport AppConfigs from './app.configs.json'\nimport MockData from '../../__mocks__/header.json'\nexport { AppConfigs, MockData }\n\ntype HeaderContextType = {\n  activeNav: boolean\n  activeSearch: boolean\n  activeMask: boolean\n  activeVerticalNav: boolean\n  PlatformComponents: any\n  PlatformHooks: any\n  setActiveMask: (value: boolean) => void\n  setActiveSearch: (value: boolean) => void\n  showVerticalNav: boolean\n  toggleNav: () => void\n  toggleActiveVerticalNav: (value: boolean) => void\n  content: HeaderContent | any\n  controls: HeaderControls | any\n  isAfterHeaderAndBeforeEndPage: boolean\n}\n\nexport const HeaderContext = createContext<HeaderContextType>({\n  activeNav: false,\n  activeSearch: false,\n  activeMask: false,\n  activeVerticalNav: false,\n  PlatformComponents: {},\n  PlatformHooks: {},\n  setActiveMask: () => {},\n  setActiveSearch: () => {},\n  showVerticalNav: false,\n  toggleNav: () => {},\n  toggleActiveVerticalNav: () => {},\n  isAfterHeaderAndBeforeEndPage: false,\n  content: {},\n  controls: {},\n})\n\nconst Header: React.FC<HeaderProps> = ({\n  data,\n  PlatformComponents,\n  PlatformHooks,\n}) => {\n  const [activeNav, setActiveNav] = useState(false)\n  const [activeMask, setActiveMask] = useState(false)\n  const [activeSearch, setActiveSearch] = useState(false)\n  const [activeVerticalNav, toggleActiveVerticalNav] = useState(false)\n  const [\n    isAfterHeaderAndBeforeEndPage,\n    setIsAfterHeaderAndBeforeEndPage,\n  ] = useState(false)\n  const scrollPosition = useScrollYPosition()\n\n  function toggleNav() {\n    const {\n      controls: {\n        general: { showVerticalNav },\n      },\n    } = data\n\n    let newActiveNav = false\n\n    if (showVerticalNav) {\n      newActiveNav = !activeVerticalNav\n      toggleActiveVerticalNav(newActiveNav)\n    } else {\n      newActiveNav = !activeNav\n      setActiveNav(newActiveNav)\n    }\n\n    if (newActiveNav) {\n      document.body.classList.add('md-overflow-hidden')\n    } else {\n      document.body.classList.remove('md-overflow-hidden')\n    }\n  }\n\n  const handleScrollPosition = (curTop: number) => {\n    const {\n      controls: {\n        general: {\n          variations: { addStickyHeaderOnMobile, addStickyHeaderOnDesk },\n        },\n      },\n    } = data\n    const isMobile = window.innerWidth < 1026\n    const $header = document.querySelector('.header') as HTMLInputElement\n    const $footer = document.querySelector('.footer') as HTMLInputElement\n\n    if (\n      (!isMobile && addStickyHeaderOnDesk) ||\n      (isMobile && addStickyHeaderOnMobile)\n    ) {\n      if (curTop > 0) {\n        $header.classList.add('header--sticky')\n      } else {\n        $header.classList.remove('header--sticky')\n      }\n    }\n\n    if (!isMobile) return\n\n    if (\n      curTop > $header.clientHeight &&\n      curTop < $footer.offsetTop - window.screen.height\n    ) {\n      setIsAfterHeaderAndBeforeEndPage(true)\n    } else {\n      setIsAfterHeaderAndBeforeEndPage(false)\n    }\n  }\n\n  useEffect(() => {\n    let $header = document.querySelector('.header') as HTMLInputElement\n    let $footer = document.querySelector('.footer') as HTMLInputElement\n\n    if (!$header || !$footer) {\n      const interval = setInterval(() => {\n        $header = document.querySelector('.header') as HTMLInputElement\n        $footer = document.querySelector('.footer') as HTMLInputElement\n\n        if ($header && $footer) {\n          clearInterval(interval)\n          handleScrollPosition(scrollPosition)\n        }\n      }, 1000)\n    } else {\n      handleScrollPosition(scrollPosition)\n    }\n  }, [scrollPosition])\n\n  if (!data) return null\n\n  const {\n    content: { logo, logoLink },\n    controls: {\n      general: { showVerticalNav },\n      style: { backgroundColor },\n    },\n  } = data\n\n  return (\n    <HeaderContext.Provider\n      value={{\n        activeNav,\n        activeSearch,\n        activeMask,\n        activeVerticalNav,\n        content: data.content,\n        controls: data.controls,\n        PlatformComponents,\n        PlatformHooks,\n        setActiveMask,\n        setActiveSearch,\n        showVerticalNav,\n        toggleNav,\n        toggleActiveVerticalNav,\n        isAfterHeaderAndBeforeEndPage,\n      }}\n    >\n      <div\n        className=\"header relative z-10\"\n        style={{\n          backgroundColor: backgroundColor,\n        }}\n      >\n        <div className=\"header-row bg-white relative pt-2.5 md:py-2.5\">\n          <div className=\"container\">\n            <div className=\"flex items-center justify-between flex-wrap\">\n              <Burger />\n              {logo ? (\n                <a\n                  className=\"header-logo\"\n                  href={logoLink?.url || '/'}\n                  target={logoLink.target}\n                >\n                  <LazyImage\n                    className=\"max-w-[150px] md:max-w-[200px]\"\n                    image={logo}\n                  />\n                </a>\n              ) : null}\n\n              <div className=\"hidden md:block w-2/5 relative z-30\">\n                {PlatformComponents?.SearchInput && (\n                  <PlatformComponents.SearchInput />\n                )}\n              </div>\n\n              <div className=\"flex items-center space-x-5\">\n                <Profile />\n                <Wishlist />\n                {PlatformComponents?.CartToggle && (\n                  <PlatformComponents.CartToggle />\n                )}\n              </div>\n\n              <div className=\"block w-full md:hidden mt-2.5 text-sm\">\n                {PlatformComponents?.SearchInput && (\n                  <PlatformComponents.SearchInput />\n                )}\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <Mask />\n\n        <Nav />\n\n        <VerticalNav />\n\n        <BottomMobileNav />\n      </div>\n    </HeaderContext.Provider>\n  )\n}\n\nexport default Header\n","import React, { useEffect, useState } from 'react'\nimport InputMask from 'react-input-mask'\nimport cn from 'classnames'\n\nimport { InputTextProps } from './types'\n\nconst InputText: React.FC<InputTextProps> = ({\n  autoComplete,\n  className,\n  errorMessage,\n  hasError,\n  id,\n  label,\n  mask = '',\n  name,\n  type,\n  onChange,\n  value,\n  ...props\n}) => {\n  const [inputError, setInputError] = useState(false)\n\n  useEffect(() => {\n    if (hasError?.[name]) {\n      setInputError(true)\n    } else {\n      setInputError(false)\n    }\n  }, [hasError, name])\n\n  if (type === 'textarea') {\n    return <textarea {...props} />\n  }\n\n  const inputClass = cn('w-full bg-white relative text-xs outline-0 p-3 text-black border disabled:opacity-30', {\n    ['input-text__field--error border-danger']: hasError?.[name],\n  })\n\n  return (\n    <>\n      {mask ? (\n        <div className={cn(\"input-text\", className)}>\n          {label && <label className=\"block mb-1 text-sm\">\n            {label}\n          </label>}\n          <InputMask\n            autoComplete={autoComplete}\n            className={inputClass}\n            id={id}\n            mask={mask}\n            name={name}\n            onChange={onChange}\n            type={type}\n            value={value}\n            {...props}\n          />\n        </div>\n      ) : (\n        <div\n          className={cn('input-text', className, {\n            ['input-text--error']: inputError\n          })}\n        >\n          {label &&\n            <label className=\"block mb-1 text-sm\">\n              {label}\n            </label>}\n          <input\n            className={inputClass}\n            name={name}\n            onChange={onChange}\n            type=\"text\"\n            value={value}\n            {...props}\n          />\n        </div>\n      )}\n      {inputError && errorMessage && (\n        <p className=\"input-text__error-message text-danger text-left text-xs\">{errorMessage}</p>\n      )}\n    </>\n  )\n}\n\nexport default InputText;\n\n","import React, { useState, useEffect, useRef } from 'react'\nimport cn from 'classnames'\nimport { InputCheckProps } from './types'\n\nconst InputCheck: React.FC<InputCheckProps> = ({\n  className,\n  color = 'text-white',\n  dataValidation,\n  disabled,\n  hasError,\n  errorMessage,\n  label,\n  name,\n  onClick,\n  value,\n  ...props\n}) => {\n  const [inputError, setInputError] = useState(false)\n  const input: any = useRef(null)\n\n  function getValue() {\n    return input.current.checked\n  }\n\n  useEffect(() => {\n    if (hasError && hasError[name]) {\n      setInputError(true)\n    } else {\n      setInputError(false)\n    }\n  }, [hasError, name, value])\n\n  return (\n    <div className={cn(\"input-check flex items-center flex-wrap text-xs\", color, className)}>\n      <label className={`input-check__label flex cursor-pointer`}>\n        <input\n          className='hidden'\n          data-validation={dataValidation}\n          name={name}\n          onChange={() => onClick && onClick(getValue())}\n          ref={input}\n          type=\"checkbox\"\n          value={value}\n          {...props}\n        />\n        <span className={cn('input-check__box border-2 w-[18px] h-[18px] rounded flex items-center justify-center', {\n          'before:content-[\"\"] before:block before:rotate-45 before:translate-[-1px, -1px] before:h-[10px] before:w-[6px] before:border-b-2 before:border-r-2 before:relative before:top-[-1px]': value,\n          'border-red': inputError,\n          'opacity-30': disabled\n        })} />\n        {label && <span\n          className=\"input-check__text ml-2\"\n          dangerouslySetInnerHTML={{ __html: label }}\n        />}\n      </label>\n      {inputError && errorMessage && (\n        <p className={cn(\"input-check__error-msg w-full mt-1\", {\n          'text-error': inputError\n        })}>{errorMessage}</p>\n      )}\n    </div>\n  )\n}\n\nexport default InputCheck\n","import React from 'react'\nimport PropTypes from 'prop-types'\nimport ReactDOM from 'react-dom'\nimport cn from 'classnames'\n\nconst Modal: React.FC<any> = ({\n  className,\n  children,\n  fadeIn,\n  onClose,\n  removeCloseBtn,\n  show,\n  style,\n}) => {\n  function closeBtn() {\n    if (removeCloseBtn) return null\n\n    return (\n      <span\n        className=\"modal__close\"\n        aria-hidden=\"true\"\n        onClick={() => onClose()}\n      />\n    )\n  }\n\n  if ((!fadeIn && !show)) {\n    return null\n  }\n\n  return ReactDOM.createPortal(\n    <div className={cn('modal fixed w-full h-full top-0 flex items-center justify-center duration-300', className, {\n      'opacity-0 invisible': !show,\n      'open visible opacity-100': show,\n    })}>\n      <div className={cn(\"modal-mask fixed top-0 left-0 bg-black bg-opacity-60 w-screen h-screen ease duration-200\", {\n        \"opacity-100 visible\": show,\n        \"opacity-0 w-100 invisible\": !show\n      })} onClick={() => onClose()} />\n      <div\n        className=\"modal__content max-w-80 relative\"\n        style={style}\n      >\n        {closeBtn()}\n        {children}\n      </div>\n    </div>, document.body)\n}\n\nModal.propTypes = {\n  className: PropTypes.string,\n  children: PropTypes.node.isRequired,\n  effects: PropTypes.bool,\n  onClose: PropTypes.func,\n  show: PropTypes.bool,\n}\n\nexport default Modal\n","export function validate(rule: string, value: string): boolean {\n  switch (rule) {\n    case 'name':\n      return /^[a-z\\s]+$/i.test(value)\n\n    case 'email':\n      return /^[\\w._+-]+(\\.\\w+)?@\\w+\\.[\\w]+(\\.[\\w]+)?$/gim.test(value)\n\n    case 'required':\n      return value.trim() !== ''\n\n    case 'checkbox':\n      return !(!value || value === 'false')\n\n    case 'zipcode':\n      return /\\d{5}-\\d{3}/gim.test(value)\n\n    default:\n      return false\n  }\n}\n\nexport function fieldValidation(element: HTMLInputElement): boolean | unknown {\n  if (!element?.getAttribute) return\n\n  const validation = element.getAttribute('data-validation')\n  const inputValue = element.value.trim()\n\n  if (!validation) return\n\n  if (\n    (/required/.test(validation) && (!inputValue || inputValue.length <= 0)) ||\n    (/email/.test(validation) && !validate('email', inputValue))\n  ) {\n    return true\n  }\n\n  return false\n}\n\nexport default {\n  validate,\n  fieldValidation,\n}\n","import React, { FormEvent, useRef } from 'react'\n\nimport * as ValidationsUtils from '../../utils/validations'\nimport { FormProps } from './types'\n\nconst Form: React.FC<FormProps> = ({\n  action,\n  className,\n  children,\n  method,\n  onSubmit,\n  ...props\n}) => {\n  const formRef = useRef(null)\n\n  function submitHandler(e: FormEvent<HTMLFormElement>) {\n    e.preventDefault()\n\n    let formError: any = {}\n    let isValid = true\n\n    const $validatedInputs = (\n      e.target as HTMLTextAreaElement\n    )?.querySelectorAll('[data-validation]')\n\n    const updateErrorState = (inputName: string) => {\n      const tempFormError = {\n        ...formError,\n        [inputName]: true,\n      }\n\n      isValid = false\n      formError = tempFormError\n\n      return formError\n    }\n\n    $validatedInputs.forEach(($input: any) => {\n      const validation = $input.getAttribute('data-validation')\n      const inputName = $input.getAttribute('name')\n      const inputValue = $input.value.trim()\n\n      if (\n        /required/.test(validation) &&\n        !ValidationsUtils.validate('required', inputValue)\n      )\n        updateErrorState(inputName)\n      if (\n        /email/.test(validation) &&\n        !ValidationsUtils.validate('email', inputValue)\n      )\n        updateErrorState(inputName)\n      if (\n        /checkbox/.test(validation) &&\n        !ValidationsUtils.validate('checkbox', inputValue)\n      ) {\n        updateErrorState(inputName)\n      }\n      if (\n        /zipcode/.test(validation) &&\n        !ValidationsUtils.validate('zipcode', inputValue)\n      ) {\n        updateErrorState(inputName)\n      }\n    })\n\n    onSubmit?.(e, isValid, formError)\n  }\n\n  return (\n    <form\n      action={action}\n      className={className}\n      method={method}\n      onSubmit={submitHandler}\n      ref={formRef}\n      {...props}\n    >\n      {children}\n    </form>\n  )\n}\n\nexport default Form\n","/* eslint-disable no-console */\nimport axios from 'axios'\n\nexport async function fetchData(\n  url: string,\n  data?: any,\n  method: any = 'get',\n  headers?: any\n) {\n  const axiosConfig = {\n    url,\n    method: method ?? 'get',\n    headers,\n    data: JSON.stringify(data) ?? null,\n  }\n\n  const response = await axios(axiosConfig)\n\n  return response.data\n}\n\nexport async function fetchWordPressData(\n  apiBaseUrl: string,\n  slug: string\n) {\n  return axios({\n    url: `${apiBaseUrl}/${slug}`,\n    method: 'GET',\n  }).then(response => response.data)\n}\n\nexport async function fetchWordPressDataFromPath(\n  apiBaseUrl: string,\n  path: string,\n  pathFallback?: string\n) {\n  let response: any = null\n  const pathName = path.replace(/^\\/|\\/$/g, '')\n  const pathSlugs = pathName.split('/')\n\n  if (pathName) {\n    response = await fetchWordPressData(apiBaseUrl, `page/${pathName}`)\n  }\n\n  // try to get first page from slug\n  // for example: feminino/acessorios\n  // try to get feminino endpoint\n  if (\n    (!response || (response && response.status === 404)) &&\n    pathSlugs[0] &&\n    pathSlugs[0] !== pathName\n  ) {\n    response = await fetchWordPressData(apiBaseUrl, `page/${pathSlugs[0]}`)\n  }\n\n  if (!response || response.status === 404 || (!response.components || !response.components.length)) {\n    response = await fetchWordPressData(apiBaseUrl, `page/${pathFallback}`)\n  }\n\n  return response\n}\n\nexport default {\n  fetchData,\n  fetchWordPressData,\n  fetchWordPressDataFromPath,\n}\n","import * as FormatsUtils from '../formats'\nimport { fetchData } from '../../api'\n\nexport async function getUserProfile(apiBaseUrl: string) {\n  return fetchData(`${apiBaseUrl}/no-cache/profileSystem/getProfile`).then(\n    (response: any) => response\n  )\n}\n\nexport async function getOrdersFromUser(apiBaseUrl: string) {\n  return fetchData(`${apiBaseUrl}/api/checkout/pub/orders`).then((response: any) => response)\n}\n\nexport async function getBrands(apiBaseUrl: string) {\n  return fetchData(`${apiBaseUrl}/api/catalog_system/pub/brand/list`).then(\n    (response: any) => response\n  )\n}\n\nexport async function getProductById(apiBaseUrl: string, id: any) {\n  return fetchData(\n    `${apiBaseUrl}/api/catalog_system/pub/products/search/?fq=productId:${id}`\n  ).then(response =>\n    response.length ? FormatsUtils.productData(response[0]) : null\n  )\n}\n\nexport async function getSimilarsFromProduct(apiBaseUrl: string, id: number) {\n  return fetchData(\n    `${apiBaseUrl}/api/catalog_system/pub/products/crossselling/similars/${id}`\n  ).then((response: any) => response)\n}\n\nexport async function getProductFromCategory(apiBaseUrl: string, page = 1) {\n  // const catId = window.vtxctx ? window.vtxctx.categoryId : 0;\n  const catId = 0\n  const itemsPerPage = 24\n  const start = page === 1 ? 1 : (page - 1) * itemsPerPage\n  const end = itemsPerPage * page\n\n  return fetchData(\n    `${apiBaseUrl}/api/catalog_system/pvt/products/GetProductAndSkuIds?categoryId=${catId}&_from=${start}&_to=${end}`\n  ).then((response: any) => response)\n}\n\nexport async function postCartSimulation(apiBaseUrl: string, data: any) {\n  return fetchData(\n    `${apiBaseUrl}/api/checkout/pub/orderforms/simulation`,\n    data,\n    'post'\n  ).then(response =>\n    response.length ? FormatsUtils.productData(response[0]) : null\n  )\n}\n\nexport async function getOrderForm(apiBaseUrl: string) {\n  return fetchData(`${apiBaseUrl}/api/checkout/pub/orderForm`).then(response =>\n    FormatsUtils.orderForm(response)\n  )\n}\n\nexport async function getShipping(apiBaseUrl:string, data: any) {\n  try {\n    const orderForm: any = await getOrderForm(apiBaseUrl)\n\n    return fetchData(\n      `${apiBaseUrl}/api/checkout/pub/orderforms/simulation?sc=${orderForm.salesChannel}`,\n      data,\n      'post'\n    ).then(response => response)\n  } catch (error) {\n    return null\n  }\n}\n\nexport async function getMasterDataEntitie(url: string) {\n  return await fetchData(url)\n}\n\nexport async function saveMasterData(apiBaseUrl:string ,data: any, action?: string) {\n  const { acronym, ...payload } = data\n\n  const promise = new Promise((resolve, reject) => {\n    /* eslint-disable-next-line */\n    ; (async () => {\n      try {\n        if (action === 'newslletter') {\n          const registerExist = await getMasterDataEntitie(\n            `${apiBaseUrl}/api/dataentities/${acronym}/search?_where=email=${payload.email}&_fields=email`\n          )\n\n          if (registerExist.length) {\n            resolve('error')\n          }\n        }\n\n        try {\n          await fetchData(\n            `${apiBaseUrl}/api/dataentities/${acronym}/documents`,\n            payload,\n            'post'\n          ).then(() => {\n            resolve('success')\n          })\n        } catch {\n          resolve('apiError')\n        }\n      } catch (err) {\n        reject(err)\n      }\n    })()\n  })\n\n  return promise\n}\n\nexport async function sendDataWarnMe(apiBaseUrl: string, data: any) {\n  return fetch(`${apiBaseUrl}/no-cache/AviseMe.aspx`, {\n    method: 'post',\n    headers: new Headers(),\n    body: data,\n  }).then(response => response)\n}\n\nexport async function shippingCalc(apiBaseUrl: string, obj: any) {\n  const promise = new Promise(resolve => {\n    getShipping(apiBaseUrl, obj).then(result => {\n      const data: any = []\n\n      if (\n        result &&\n        result.logisticsInfo.length &&\n        result.logisticsInfo[0].slas.length\n      ) {\n        let pickupInPoint = false\n\n        result.logisticsInfo[0].slas.forEach((sla: any) => {\n          if (sla.deliveryChannel === 'delivery' || !pickupInPoint) {\n            sla.deliveryChannel === 'pickup-in-point'\n              ? (pickupInPoint = true)\n              : ''\n\n            const price =\n              sla.price === 0\n                ? 'Grátis'\n                : (sla.price / 100).toLocaleString('pt-br', {\n                  style: 'currency',\n                  currency: 'BRL',\n                })\n\n            // NEED REFACTOR\n            // const days = sla.shippingEstimate.includes('bd')\n            //   ? sla.shippingEstimate.replace('bd', '') + ' ' +\n            //     (sla.shippingEstimate.replace('bd', '') <= 1 ? 'dia' : 'dias')\n            //   : sla.shippingEstimate.replace('h', '') +\n            //     ' ' +\n            //     (sla.shippingEstimate.replace('h', '') <= 1 ? 'hora' : 'horas')\n\n            const name =\n              sla.deliveryChannel === 'pickup-in-point'\n                ? 'Retire em loja'\n                : sla.name\n\n            data.push({\n              price,\n              days: 1,\n              name,\n            })\n          }\n        })\n      }\n\n      resolve(data)\n    })\n  })\n\n  return promise\n}\n\nexport async function getPageCategoryHTML(url: string) {\n  return fetchData(url).then(res => {\n    const data: any = {\n      productIds: [],\n      layout: '',\n    }\n\n    res = new DOMParser().parseFromString(res, 'text/html')\n    const productsShelf = Array.from(res.querySelectorAll('[data-id]')) as any\n\n    if (productsShelf.length) {\n      productsShelf.forEach((el: HTMLElement) => {\n        const paramId = el.getAttribute('data-id')\n\n        if (paramId) {\n          data.productIds.push({\n            id: parseInt(paramId, 10),\n          })\n        }\n      })\n      data.layout = productsShelf[0].getAttribute('data-template')\n    }\n\n    return data\n  })\n}\n\nexport async function getSimilarProducts(apiBaseUrl: string, id: any) {\n  return fetchData(\n    `${apiBaseUrl}/api/catalog_system/pub/products/crossselling/similars/${id}`\n  ).then((response: any) => response)\n}\n\nexport async function getCrossSellingProducts(\n  apiBaseUrl: string,\n  name: string,\n  productId: number\n) {\n  return fetchData(\n    `${apiBaseUrl}/api/catalog_system/pub/products/crossselling/${name}/${productId}`\n  ).then((response: any) => response)\n}\n\nexport async function getRatingByProductId(storeId: string, productId: any) {\n  return fetchData(`https://${storeId}.myvtex.com/reviews-and-ratings/api/reviews?product_id=${productId}`).then((response: any) => response)\n}\n\nexport default {\n  getUserProfile,\n  getOrdersFromUser,\n  getBrands,\n  getProductById,\n  getSimilarsFromProduct,\n  getProductFromCategory,\n  postCartSimulation,\n  getOrderForm,\n  getShipping,\n  getMasterDataEntitie,\n  saveMasterData,\n  sendDataWarnMe,\n  getPageCategoryHTML,\n  getSimilarProducts,\n  shippingCalc,\n  getCrossSellingProducts,\n  getRatingByProductId\n}\n","import Swal from 'sweetalert2'\n\nexport default function showMessageModal(\n  name: string,\n  variation: string,\n): void {\n  let sucessMessage = ''\n  let errorMessage = 'Ocorreu um erro, tente novamente'\n\n  switch (variation) {\n    case 'newsletter':\n      sucessMessage =\n        'Bem vindo, em breve você receberá nossas promoções e novidades por email.'\n      errorMessage = 'E-mail cadastrado, tente novamente'\n      break\n    case 'contact':\n      sucessMessage = 'Em breve retornaremos o contato'\n      break\n    default:\n      return\n  }\n\n  const messageModals: any = {\n    name: () => Swal.fire('Oops!', 'Digite seu nome', 'error'),\n    email: () => Swal.fire('Oops!', 'Digite um email válido', 'error'),\n    terms: () =>\n      Swal.fire(\n        'Oops!',\n        'Aceite os termos de política de privacidade',\n        'error',\n      ),\n    success: () => Swal.fire('Obrigado!', sucessMessage, 'success'),\n    error: () => Swal.fire('Oops!', errorMessage, 'error'),\n    apiError: () =>\n      Swal.fire('Oops!', 'Ocorreu um erro, tente novamente', 'error'),\n  }\n\n  messageModals[name] ? messageModals[name]() : null\n}\n","import React from 'react'\n\nconst LoadingBricks: React.FC<any> = ({\n  className,\n  height,\n  margin,\n  width,\n  ...rest\n}) => (\n  <div\n    className={className ? `loading ${className}` : 'loading'}\n    style={{ width, height, margin, ...rest }}\n  />\n)\n\nLoadingBricks.defaultProps = {\n  height: '100%',\n  margin: '0px 0px 10px 0px',\n  width: '100%',\n}\n\nexport default LoadingBricks\n","import Accordion, {\n  MockData as AccordionMockData,\n  AppConfigs as AccordionAppConfigs,\n} from \"./accordion\";\nimport Banner, {\n  MockData as BannerMockData,\n  AppConfigs as BannerAppConfigs,\n} from \"./banner\";\nimport Bannergrid, {\n  MockData as BannergridMockData,\n  AppConfigs as BannergridAppConfigs,\n} from \"./bannergrid\";\nimport Countdown, {\n  MockData as CountdownMockData,\n  AppConfigs as CountdownAppConfigs,\n} from \"./countdown\";\nimport Carousel from \"./carousel\";\nimport Container from \"./container\";\nimport Footer, {\n  MockData as FooterMockData,\n  AppConfigs as FooterAppConfigs,\n} from \"./footer\";\nimport Header, {\n  MockData as HeaderMockData,\n  AppConfigs as HeaderAppConfigs,\n} from \"./header\";\nimport InputText from \"./input-text\";\nimport InputCheck from \"./input-check\";\nimport Modal from \"./modal\";\nimport Newsletter, {\n  MockData as NewsletterMockData,\n  AppConfigs as NewsletterAppConfigs,\n} from \"./newsletter\";\nimport StoresIndex from \"./stores-index\";\nimport Stores, {\n  MockData as StoresMockData,\n  AppConfigs as StoresAppConfigs,\n} from \"./stores\";\nimport Tipbar, {\n  MockData as TipbarMockData,\n  AppConfigs as TipbarAppConfigs,\n} from \"./tipbar\";\nimport HeaderbarMini from \"./headerbar-mini\";\nimport ProductDetails, {\n  MockData as ProductDetailsMockData,\n  AppConfigs as ProductDetailsAppConfigs,\n} from \"./product-details\";\nimport ProductShelf, {\n  MockData as ProductShelfMockData,\n  AppConfigs as ProductShelfAppConfigs,\n} from \"./product-shelf\";\nimport CrossSellingShelf, {\n  MockData as CrossSellingShelfMockData,\n  AppConfigs as CrossSellingShelfAppConfigs,\n} from \"./cross-selling-shelf\";\nimport ProductGallery, {\n  MockData as ProductGalleryMockData,\n  AppConfigs as ProductGalleryAppConfigs,\n} from \"./product-gallery\";\nimport MosaicBanner from \"./mosaic-banner\";\n\nimport Wishlist, {\n  MockData as WishlistMockData,\n  AppConfigs as WishlistAppConfigs,\n} from \"./wishlist\";\n\nexport default {\n  // accordion\n  Accordion,\n  AccordionMockData,\n  AccordionAppConfigs,\n\n  // banner\n  Banner,\n  BannerMockData,\n  BannerAppConfigs,\n\n  // bannergrid\n  Bannergrid,\n  BannergridMockData,\n  BannergridAppConfigs,\n\n  Carousel,\n  Container,\n\n  // countdown\n  Countdown,\n  CountdownMockData,\n  CountdownAppConfigs,\n\n  // cross selling shelf\n  CrossSellingShelf,\n  CrossSellingShelfMockData,\n  CrossSellingShelfAppConfigs,\n\n  // footer\n  Footer,\n  FooterMockData,\n  FooterAppConfigs,\n\n  // header\n  Header,\n  HeaderMockData,\n  HeaderAppConfigs,\n\n  HeaderbarMini,\n  InputText,\n  InputCheck,\n  Modal,\n\n  // newsletter\n  Newsletter,\n  NewsletterMockData,\n  NewsletterAppConfigs,\n\n  // product details\n  ProductDetails,\n  ProductDetailsMockData,\n  ProductDetailsAppConfigs,\n\n  // product gallery\n  ProductGallery,\n  ProductGalleryMockData,\n  ProductGalleryAppConfigs,\n\n  // product shelf\n  ProductShelf,\n  ProductShelfMockData,\n  ProductShelfAppConfigs,\n\n  // stores\n  Stores,\n  StoresMockData,\n  StoresAppConfigs,\n\n  StoresIndex,\n\n  // tipbar\n  Tipbar,\n  TipbarMockData,\n  TipbarAppConfigs,\n\n  // wishlist\n  Wishlist,\n  WishlistMockData,\n  WishlistAppConfigs,\n\n  MosaicBanner,\n};\n","import React, { FC, useState } from 'react'\nimport cn from 'classnames'\n\nimport AccordionItem from './item'\nimport type { AccordionProps, AccordionItemProps } from './types'\n\n// IT'S VERY IMPORTANT\n// without it the app won't render the components configs properly\n// export app configs json file & mock data\nimport AppConfigs from './app.configs.json'\nimport MockData from '../../__mocks__/accordion.json'\nexport { AppConfigs, MockData }\n\nconst Accordion: FC<AccordionProps> = ({\n  content: { items },\n  controls: {\n    general: { showIcon },\n    style: { customClass, customIconClass },\n  },\n}) => {\n  const [activeIndex, setActiveIndex] = useState(-1)\n\n  if (!items?.length) return null\n\n  return (\n    <div className={cn('accordion', customClass)}>\n      <div className=\"accordion__items\">\n        {items.map((item: AccordionItemProps, index: number) => (\n          <AccordionItem\n            activeIndex={activeIndex}\n            content={item.content}\n            customIconClass={customIconClass}\n            index={index}\n            title={item.title}\n            onActiveIndex={setActiveIndex}\n            showIcon={showIcon}\n            key={index}\n          />\n        ))}\n      </div>\n    </div>\n  )\n}\n\nexport default Accordion\n","import React, { useCallback } from 'react'\nimport cn from 'classnames'\n\nimport { BannerProps, BannerPropsItem } from './types'\nimport Button from '../button'\nimport createEffectClasses from '../../helpers/create-effect-classes'\nimport Carousel from '../carousel'\nimport Container from '../container'\n\n// IT'S VERY IMPORTANT\n// without it the app won't render the components configs properly\n// export app configs json file\nimport AppConfigs from './app.configs.json'\nimport MockData from '../../__mocks__/banner.json'\nexport { AppConfigs, MockData }\n\nconst Banner: React.FC<BannerProps> = ({\n  content: { items },\n  controls: {\n    general: {\n      applyMobileCarouselControls,\n      carouselControls,\n      deskVariations,\n      effects: { hoverArrows, ...effectsProps },\n      mobileCarouselControls,\n      mobileVariations,\n    },\n    style: { backgroundColor, customClass, color },\n  },\n  PlatformComponents,\n}) => {\n  const renderImages = ({ mobileImage, deskImage }: BannerPropsItem) => {\n    if (!mobileImage && !deskImage) return null\n\n    const { url, width, height, alt } = mobileImage || deskImage\n\n    return (\n      <picture>\n        <source srcSet={deskImage?.url} media=\"(min-width: 1026px)\" />\n        {\n          <PlatformComponents.Image\n            image={url}\n            width={width}\n            height={height}\n            alt={alt}\n            preload\n            loading=\"eager\"\n            fetchPriority=\"high\"\n          />\n        }\n      </picture>\n    )\n  }\n\n  const renderBannerItem = useCallback(\n    (item: BannerPropsItem) => {\n      const bannerItemClasses = cn(\n        'banner__item',\n        createEffectClasses(effectsProps)\n      )\n\n      const videoContent = (\n        <>\n          <video autoPlay loop playsInline muted className=\"lg:hidden\">\n            <source\n              src={item.mobileVideo?.url || item.deskVideo?.url}\n              type=\"video/mp4\"\n            />\n          </video>\n          <video autoPlay loop playsInline muted className=\"hidden lg:block\">\n            <source src={item.deskVideo?.url} type=\"video/mp4\" />\n          </video>\n        </>\n      )\n\n      if (item.isVideo) {\n        if (item.link) {\n          return (\n            <Button\n              href={item.link.url}\n              target={item.link.target}\n              PlatformComponents={PlatformComponents}\n              className={bannerItemClasses}\n            >\n              {videoContent}\n            </Button>\n          )\n        }\n\n        return videoContent\n      }\n\n      return (\n        <article className={bannerItemClasses} id={item.id}>\n          {item.link?.url ? (\n            <figure className=\"banner__figure\">\n              <Button\n                href={item.link.url}\n                target={item.link.target}\n                PlatformComponents={PlatformComponents}\n              >\n                {renderImages(item)}\n              </Button>\n            </figure>\n          ) : (\n            <figure className=\"banner__figure\">{renderImages(item)}</figure>\n          )}\n\n          {item.showDesc && (\n            <div\n              className=\"banner__content rich-text p-6 bg-light text-center\"\n              style={{\n                backgroundColor,\n                color,\n              }}\n              dangerouslySetInnerHTML={{ __html: item.desc }}\n            />\n          )}\n        </article>\n      )\n    },\n    [effectsProps, backgroundColor, color]\n  )\n\n  if (!items?.length) {\n    return null\n  }\n\n  return (\n    <div\n      className={cn('banner', customClass, {\n        'mobile-only:hidden': mobileVariations.hide,\n        'lg:hidden': deskVariations.hide,\n      })}\n    >\n      <Container\n        className={cn({\n          'mobile-only:p-0': mobileVariations.full,\n          'md:p-0': deskVariations.full,\n          effect: hoverArrows,\n        })}\n      >\n        <div className=\"banner__items\">\n          <Carousel\n            {...carouselControls}\n            applyMobileCarouselControls={applyMobileCarouselControls}\n            mobileCarouselControls={mobileCarouselControls}\n            hoverArrows={hoverArrows}\n          >\n            {items.map((item: BannerPropsItem, index: number) => (\n              <div key={index}>{renderBannerItem(item)}</div>\n            ))}\n          </Carousel>\n        </div>\n      </Container>\n    </div>\n  )\n}\n\nexport default Banner\n","/* eslint-disable radix */\nimport React, { PropsWithChildren } from 'react'\nimport cn from 'classnames'\n\nimport HeaderbarMini from '../headerbar-mini'\nimport Button from '../button'\nimport LazyImage from '../lazy-image'\nimport type {\n  BannergridProps,\n  BannergridPropsItem,\n  BannergridFeaturedsItem,\n} from './models'\nimport createEffectClasses from '../../helpers/create-effect-classes'\nimport Carousel from '../carousel'\n\n// IT'S VERY IMPORTANT\n// without it the app won't render the components configs properly\n// export app configs json file\nimport AppConfigs from './app.configs.json'\nimport MockData from '../../__mocks__/bannergrid.json'\nimport Container from '../container'\nexport { AppConfigs, MockData }\n\nconst BannerGrid: React.FC<BannergridProps> = ({\n  content: { items, title, link, sidebarText },\n  controls: {\n    general: {\n      applyMobileCarouselControls,\n      carouselControls,\n      deskVariations,\n      effects: { hoverArrows, ...effectsProps },\n      mobileCarouselControls,\n      mobileVariations,\n    },\n    style: { customClass, backgroundColor, color },\n  },\n  PlatformComponents,\n}) => {\n  const Image = ({\n    mobileImage,\n    deskImage,\n    showDesc,\n    desc = '',\n  }: BannergridPropsItem | BannergridFeaturedsItem) => {\n    if (!mobileImage && !deskImage) {\n      return null\n    }\n\n    return (\n      <>\n        <figure className=\"bannergrid__figure\">\n          <picture>\n            <source srcSet={deskImage.url} media=\"(min-width: 1026px)\" />\n            <LazyImage\n              className=\"block md:hidden\"\n              image={mobileImage || deskImage}\n            />\n            <LazyImage className=\"hidden md:block\" image={deskImage} />\n          </picture>\n        </figure>\n        {showDesc && (\n          <div\n            className=\"bannergrid__desc\"\n            dangerouslySetInnerHTML={{ __html: desc }}\n          />\n        )}\n      </>\n    )\n  }\n\n  const BannergridWrapper = ({ children }: PropsWithChildren<unknown>): any => {\n    return (\n      <div className=\"bannergrid__wrapper effect\">\n        <Carousel\n          {...carouselControls}\n          applyMobileCarouselControls={applyMobileCarouselControls}\n          mobileCarouselControls={mobileCarouselControls}\n          hoverArrows={hoverArrows}\n          className={cn({\n            'md:hidden': !carouselControls.showCarousel,\n            'md:block': carouselControls.showCarousel,\n            'mobile-only:hidden': !mobileCarouselControls.showCarousel,\n            'mobile-only:block': mobileCarouselControls.showCarousel,\n          })}\n        >\n          {children}\n        </Carousel>\n        <div\n          className={cn('flex-col md:flex-row', {\n            'md:gap-[15px]': !deskVariations.removeMargin,\n            'md:flex': !carouselControls.showCarousel,\n            'md:hidden': carouselControls.showCarousel,\n            'mobile-only:hidden': mobileCarouselControls.showCarousel,\n            'mobile-only:gap-[15px]': !mobileVariations.removeMargin,\n            'mobile-only:flex': !mobileCarouselControls.showCarousel,\n          })}\n        >\n          {children}\n        </div>\n      </div>\n    )\n  }\n\n  const effectClasses = createEffectClasses(effectsProps)\n\n  if (!items?.length) return null\n\n  return (\n    <div\n      className={cn('bannergrid', customClass, {\n        'mobile-only:hidden': mobileVariations.hide,\n        'lg:hidden': deskVariations.hide,\n      })}\n      style={{\n        backgroundColor,\n        color,\n      }}\n    >\n      <HeaderbarMini title={title} link={link} sidebarText={sidebarText} />\n      <Container\n        className={cn({\n          'mobile-only:p-0': mobileVariations.full,\n          'lg:p-0': deskVariations.full,\n          effect: hoverArrows,\n        })}\n      >\n        <BannergridWrapper>\n          {items.map(\n            (\n              item: BannergridPropsItem | BannergridFeaturedsItem,\n              itemIndex: number\n            ) => (\n              <article\n                className={cn('bannergrid__item', effectClasses, {\n                  'md:px-[7.5px]':\n                    carouselControls.showCarousel &&\n                    !mobileVariations.removeMargin,\n                  'mobile-only:px-[7.5px]':\n                    mobileCarouselControls.showCarousel &&\n                    !mobileVariations.removeMargin,\n                })}\n                key={`subitem-${itemIndex}`}\n              >\n                {item?.link && item?.link?.url ? (\n                  <Button\n                    rel=\"noopener\"\n                    href={item.link.url}\n                    target={item.link.target}\n                    PlatformComponents={PlatformComponents}\n                  >\n                    {Image(item)}\n                  </Button>\n                ) : (\n                  Image(item)\n                )}\n              </article>\n            )\n          )}\n        </BannergridWrapper>\n      </Container>\n    </div>\n  )\n}\n\nexport default BannerGrid\n","import React, { useState, useEffect } from 'react'\nimport { CountdownProps, Clock } from './types'\n\n// IT'S VERY IMPORTANT\n// without it the app won't render the components configs properly\n// export app configs json file\nimport AppConfigs from './app.configs.json'\nimport MockData from '../../__mocks__/countdown.json'\nexport { AppConfigs, MockData }\n\nconst Countdown: React.FC<CountdownProps> = ({\n  content: { dateTime, title },\n  controls: {\n    style: { backgroundColor, color },\n  },\n}) => {\n  const [clock, setClock] = useState<Clock | null>(null)\n\n  function initializeClock(finalDate: any) {\n    function getTimeRemaining(endtime: any) {\n      const currentDate: any = new Date()\n      const endDate = endtime\n      const total = Date.parse(endDate) - Date.parse(currentDate)\n\n      // get total seconds between the times\n      let delta = Math.abs(total) / 1000\n\n      // calculate (and subtract) whole days\n      const days = Math.floor(delta / 86400)\n      delta -= days * 86400\n\n      // calculate (and subtract) whole hours\n      const hours = Math.floor(delta / 3600) % 24\n      delta -= hours * 3600\n\n      // calculate (and subtract) whole minutes\n      const minutes = Math.floor(delta / 60) % 60\n      delta -= minutes * 60\n\n      // what's left is seconds\n      const seconds = delta % 60\n\n      return {\n        total,\n        days: days < 10 ? `0${days}` : days,\n        hours: hours < 10 ? `0${hours}` : hours,\n        minutes: minutes < 10 ? `0${minutes}` : minutes,\n        seconds: seconds < 10 ? `0${seconds}` : seconds,\n      }\n    }\n\n    function updateClock() {\n      const tempClock: any = getTimeRemaining(finalDate)\n\n      setClock(tempClock)\n\n      if (tempClock.total <= 0) {\n        clearInterval(timeinterval)\n        setClock(null)\n      }\n    }\n\n    const timeinterval = setInterval(() => updateClock(), 1000)\n  }\n\n  useEffect(() => {\n    if (!dateTime) return\n    const date = new Date(dateTime)\n    initializeClock(date)\n  }, [dateTime])\n\n  if (!clock) return null\n\n  return (\n    <div\n      className=\"countdown\"\n      style={{\n        backgroundColor,\n        color,\n      }}\n    >\n      <div className=\"countdown__wrapper container flex flex-col md:flex-row items-center justify-between\">\n        <div\n          className=\"countdown__content rich-text mb-3 md:mb-0\"\n          dangerouslySetInnerHTML={{ __html: title }}\n        />\n        <div className=\"countdown__items flex items-center justify-between\">\n          <div className=\"countdown__item flex flex-col items-center justify-center mr-3\">\n            <strong className=\"days block text-3xl md:text-[42px]\">\n              {clock.days}\n            </strong>\n            <span className=\"smalltext text-sm\">Dias</span>\n          </div>\n          <span className=\"countdown__points text-3xl md:text-[42px] mr-3\">\n            :\n          </span>\n          <div className=\"countdown__item flex flex-col items-center justify-cente mr-3\">\n            <strong className=\"hours block text-3xl md:text-[42px]\">\n              {clock.hours}\n            </strong>\n            <span className=\"smalltext text-sm\">Horas</span>\n          </div>\n          <span className=\"countdown__points text-3xl md:text-[42px] mr-3\">\n            :\n          </span>\n          <div className=\"countdown__item flex flex-col items-center justify-center mr-3\">\n            <strong className=\"minutes block text-3xl md:text-[42px]\">\n              {clock.minutes}\n            </strong>\n            <span className=\"smalltext text-sm\">Minutos</span>\n          </div>\n          <span className=\"countdown__points text-3xl md:text-[42px] mr-3\">\n            :\n          </span>\n          <div className=\"countdown__item flex flex-col items-center justify-center\">\n            <strong className=\"seconds block text-3xl md:text-[42px]\">\n              {clock.seconds}\n            </strong>\n            <span className=\"smalltext text-sm\">Segundos</span>\n          </div>\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default Countdown\n","import React from 'react'\n\n// IT'S VERY IMPORTANT\n// without it the app won't render the components configs properly\n// export app configs json file\nimport AppConfigs from './app.configs.json'\nimport MockData from '../../__mocks__/cross-selling-shelf.json'\nexport { AppConfigs, MockData }\n\ntype Props = {\n  PlatformComponents?: any\n}\n\nconst CrossSellingShelf: React.FC<Props> = ({\n  PlatformComponents,\n  ...properties\n}) => {\n  if (!PlatformComponents?.CrossSellingShelf) return null\n\n  return <PlatformComponents.CrossSellingShelf {...properties} />\n}\n\nexport default CrossSellingShelf\n","import React, { useState, useEffect, MouseEvent } from 'react'\n\nimport * as FormatsUtils from '../../utils/formats'\nimport cn from 'classnames'\nimport StoresIndex from '../stores-index'\nimport ButtonLink from '../button'\nimport LazyImage from '../lazy-image'\nimport { FooterProps } from './types'\nimport { Style } from '../../types/style'\n\n// IT'S VERY IMPORTANT\n// without it the app won't render the components configs properly\n// export app configs json file\nimport AppConfigs from './app.configs.json'\nimport MockData from '../../__mocks__/footer.json'\nexport { AppConfigs, MockData }\n\nconst Footer: React.FC<FooterProps> = ({\n  content: { copyright, row, secondRow, storesTitle },\n  controls: {\n    general: { filterType, groupFilter, showStoresFilter, stateCategories },\n    style: {\n      firstSectionBg,\n      secondSectionBg,\n      color,\n      borderColor,\n      filterBg,\n      copyrightBg,\n    },\n  },\n  PlatformComponents,\n}) => {\n  const [showFilter, setShowFilter] = useState(false)\n\n  function toggleContent({ target }: MouseEvent<HTMLElement>) {\n    const $elem = (target as HTMLElement).parentElement?.querySelector(\n      '.footer-nav__items'\n    ) as HTMLElement\n    if ($elem) {\n      if ($elem.style.maxHeight === '0px' || !$elem.style.maxHeight) {\n        $elem.style.maxHeight = `${$elem.scrollHeight}px`\n        $elem.classList.add('mb-6')\n      } else {\n        $elem.style.maxHeight = '0px'\n        $elem.classList.remove('mb-6')\n      }\n    }\n  }\n\n  function renderColumns(data: any, variation: string) {\n    if (!data?.length) {\n      return null\n    }\n    return data.map((column: any, columnIndex: any) => {\n      return (\n        <div\n          className={cn(\n            'footer-nav mobile-only:border-b mobile-only:border-light',\n            {\n              'last:border-0': variation === 'second-section',\n              'footer-nav--accordion': column.type === 'links',\n            }\n          )}\n          style={{\n            borderColor: borderColor,\n          }}\n          key={columnIndex}\n        >\n          {variation === 'first-section' ? (\n            <>\n              <button\n                className=\"footer-nav__btn px-4 py-6 w-full text-left text-base font-medium md:hidden\"\n                onClick={(evt: MouseEvent<HTMLElement>) => toggleContent(evt)}\n              >\n                {column.title}\n              </button>\n              <h5 className=\"footer-nav__title px-4 py-6 md:p-0 md:mb-5 w-full text-left text-base font-medium mobile-only:hidden\">\n                {column.title}\n              </h5>\n            </>\n          ) : (\n            <h5 className=\"footer-nav__title px-4 py-6 md:p-0 md:mb-5 w-full text-left text-base font-medium\">\n              {column.title}\n            </h5>\n          )}\n\n          <ul\n            className={cn('footer-nav__items', {\n              'px-4 md:px-0 flex items-center flex-wrap':\n                column.type === 'images',\n              'mobile-only:pb-6': column.type === 'images',\n              'mobile-only:px-8 mobile-only:max-h-0 mobile-only:overflow-hidden mobile-only:transition-all mobile-only:duration-300':\n                column.type === 'links',\n            })}\n          >\n            {column.type === 'links' && column.links?.length\n              ? column.links.map((link: any, linkIndex: any) => (\n                  <li\n                    className=\"footer-nav__item font-light mb-5 last-of-type:mb-0 text-sm\"\n                    key={`item-${linkIndex}`}\n                  >\n                    <ButtonLink\n                      href={FormatsUtils.link(link.link.url)}\n                      target={link.link.target && link.link.target}\n                      className=\"footer-nav__btn\"\n                      key={`item-link-${linkIndex}`}\n                      PlatformComponents={PlatformComponents}\n                    >\n                      {link.link.title}\n                    </ButtonLink>\n                  </li>\n                ))\n              : null}\n\n            {column.type === 'images' && column.images?.length\n              ? column.images.map((image: any, imageIndex: any) =>\n                  image.link ? (\n                    <li\n                      className=\"footer-nav__item mt-0 mr-2.5 mb-2.5 ml-0\"\n                      key={`item-image-${imageIndex}`}\n                    >\n                      <ButtonLink\n                        className=\"footer-nav__btn\"\n                        href={image.link.url}\n                        target={image.link.target && image.link.target}\n                        key={`item-link-${imageIndex}`}\n                        PlatformComponents={PlatformComponents}\n                      >\n                        <LazyImage\n                          image={image.image}\n                          className={'footer-nav__img max-w-[50px]'}\n                        />\n                      </ButtonLink>\n                    </li>\n                  ) : (\n                    <li\n                      className=\"footer-nav__item mt-0 mr-2.5 mb-2.5 ml-0\"\n                      key={`item-image-${imageIndex}`}\n                    >\n                      <LazyImage\n                        image={image.image}\n                        className={'footer-nav__img max-w-[50px]'}\n                      />\n                    </li>\n                  )\n                )\n              : null}\n          </ul>\n        </div>\n      )\n    })\n  }\n\n  function renderRow(data: any, variation: string, rowStyle: Style) {\n    if (variation !== 'stores-index' && Object.keys(data).length === 0) {\n      return null\n    }\n\n    return (\n      <div\n        className={cn('footer__row mobile-only:mb-6 lg:py-8', {\n          'mobile-only:border-b mobile-only:border-light mobile-only:pb-6 mobile-only:mb-0':\n            variation === 'stores-index',\n          'mobile-only:mb-0': variation === 'second-section',\n          'lg:border-t lg:border-light': variation !== 'first-section',\n        })}\n        style={{\n          ...rowStyle,\n          borderColor: borderColor,\n        }}\n      >\n        <div className=\"container mobile-only:p-0\">\n          <div className=\"footer__wrapper md:flex justify-between\">\n            {variation === 'stores-index' ? (\n              <StoresIndex\n                filterType={filterType}\n                groupFilter={groupFilter}\n                PlatformComponents={PlatformComponents}\n                stateCategories={stateCategories}\n                style={{\n                  filterBg,\n                  color,\n                }}\n                title={storesTitle}\n              />\n            ) : (\n              renderColumns(data, variation)\n            )}\n          </div>\n        </div>\n      </div>\n    )\n  }\n\n  useEffect(() => {\n    if (\n      window?.location.href.search('nossas-lojas') < 0 &&\n      !!showStoresFilter\n    ) {\n      setShowFilter(true)\n    } else {\n      setShowFilter(false)\n    }\n  }, [showStoresFilter])\n\n  return (\n    <div\n      className=\"footer relative\"\n      style={{\n        color: color,\n      }}\n    >\n      {row &&\n        renderRow(row, 'first-section', {\n          backgroundColor: firstSectionBg,\n        })}\n\n      {showFilter &&\n        renderRow(null, 'stores-index', {\n          backgroundColor: filterBg,\n        })}\n\n      {secondRow &&\n        renderRow(secondRow, 'second-section', {\n          backgroundColor: secondSectionBg,\n        })}\n\n      {copyright && (\n        <div\n          className=\"copyright md:mt-0 py-6 md:py-8 text-center border-t border-light\"\n          dangerouslySetInnerHTML={{ __html: copyright }}\n          style={{\n            backgroundColor: copyrightBg,\n            borderColor: borderColor,\n          }}\n        />\n      )}\n    </div>\n  )\n}\n\nexport default Footer\n","export const link = (path: string) => {\n  if (path && path.search('http') >= 0) {\n    return path\n  }\n\n  return `${path}`\n}\n\nexport const formatLink = (url: string) =>\n  url ? url.replace(/(https|http):\\/\\/(.*?)\\//, '/') : url\n\nexport const formatPrice = (price: number, division = false) => {\n  if (!price) return ''\n  return `R$ ${(division ? price / 100 : price).toLocaleString('BR', {\n    minimumFractionDigits: 2,\n    maximumFractionDigits: 2,\n  })}`\n}\n\nexport const removeAccents = (newString: any) => {\n  if (!newString) return ''\n\n  var string = newString\n  var mapAccents: any = {\n    a: /[\\xE0-\\xE6]/g,\n    e: /[\\xE8-\\xEB]/g,\n    i: /[\\xEC-\\xEF]/g,\n    o: /[\\xF2-\\xF6]/g,\n    u: /[\\xF9-\\xFC]/g,\n    c: /\\xE7/g,\n    n: /\\xF1/g,\n  }\n\n  for (var letter in mapAccents) {\n    var regx = mapAccents[letter]\n    string = string.replace(regx, letter)\n  }\n\n  return string\n}\n\nexport default {\n  link,\n  formatLink,\n  formatPrice,\n  removeAccents,\n}\n","/* eslint-disable no-console */\nimport React, { useState } from 'react'\n\n// components\nimport Form from '../form'\nimport InputText from '../input-text'\nimport InputCheck from '../input-check'\n// utils\nimport * as ApiUtils from '../../utils/vtex/services/api'\nimport * as ValidationsUtils from '../../utils/validations'\nimport showMessageModal from '../../helpers/show-message-modal'\nimport { NewsletterProps } from './types'\nimport cn from 'classnames'\n\n// IT'S VERY IMPORTANT\n// without it the app won't render the components configs properly\n// export app configs json file\nimport MockData from '../../__mocks__/newsletter.json'\nimport AppConfigs from './app.configs.json'\nimport Container from '../container'\n\nexport { AppConfigs, MockData }\n\ninterface form {\n  [key: string]: string | boolean\n  terms: boolean\n}\n\nconst Newsletter: React.FC<NewsletterProps> = ({\n  storeConfig,\n  content: { acronym = 'NT', btnTitle, terms, title },\n  controls: {\n    general: { deskVariations, mobileVariations },\n    style: {\n      backgroundColor,\n      btnBgColor,\n      btnTextColor,\n      textColor,\n      customClass,\n    },\n  },\n}) => {\n  const [form, setForm] = useState<form>({\n    name: '',\n    email: '',\n    terms: false,\n  })\n  const [formError, setFormError] = useState({})\n  const [isDisabled, setIsDisabled] = useState(false)\n  const [loading, setLoading] = useState(false)\n\n  function changeHandler(\n    e: React.ChangeEvent<HTMLInputElement>,\n    validValue: string\n  ) {\n    const inputName = e.target.name\n    const inputValue: any = e.target.value\n\n    const patterns: any = {\n      letters: /^[a-zA-Z\\s\\b]*$/,\n      numbers: /^[0-9\\b]+$/,\n    }\n\n    if (validValue && inputValue !== '') {\n      if (!patterns[validValue].test(inputValue)) return false\n    }\n\n    setForm({\n      ...form,\n      [inputName]: inputValue,\n    })\n\n    setFormError({\n      ...formError,\n      [inputName]: ValidationsUtils.fieldValidation(e.target),\n    })\n\n    return\n  }\n\n  async function saveData(data: any) {\n    setLoading(true)\n    setIsDisabled(true)\n\n    const response = await ApiUtils.saveMasterData(\n      storeConfig.storeUrl,\n      data,\n      'newsletter'\n    )\n\n    showMessageModal(String(response), 'newsletter')\n\n    if (response === 'success') {\n      setForm({\n        name: '',\n        email: '',\n        terms: false,\n      })\n    }\n\n    setLoading(false)\n    setTimeout(() => {\n      setIsDisabled(false)\n    }, 5000)\n  }\n\n  function submitHandler(_: any, isValid: any, errorFields: any) {\n    if (!isValid) {\n      showMessageModal(Object.keys(errorFields)[0], 'newsletter')\n      setFormError(errorFields)\n    } else {\n      const payload = {\n        acronym,\n        email: form.email,\n        name: form.name,\n      }\n      saveData(payload)\n    }\n  }\n\n  return (\n    <section\n      className={cn('newsletter py-6', customClass, {\n        'mobile-only:hidden': mobileVariations.hide,\n        'lg:hidden': deskVariations.hide,\n      })}\n      style={{\n        backgroundColor: backgroundColor || 'var(--primary)',\n        color: textColor || 'white',\n      }}\n    >\n      <Container>\n        <div className=\"newsletter__wrapper md:flex items-center justify-between\">\n          {title && (\n            <h2\n              className=\"newsletter__title mb-5 text-xl text-current md:text-2xl md:w-[30%] md:pr-5\"\n              dangerouslySetInnerHTML={{ __html: title }}\n            />\n          )}\n          <Form\n            className=\"newsletter__form md:flex items-center justify-between md:w-[70%] relative\"\n            onSubmit={submitHandler}\n          >\n            <InputText\n              className=\"mb-3 md:mb-0 md:mr-3 w-full\"\n              data-validation=\"required\"\n              name=\"name\"\n              placeholder=\"Seu nome*\"\n              onChange={(e: React.ChangeEvent<HTMLInputElement>) =>\n                changeHandler(e, 'letters')\n              }\n              value={form.name}\n              hasError={formError}\n            />\n\n            <InputText\n              className=\"mb-3 md:mb-0 md:mr-3 w-full\"\n              name=\"email\"\n              data-validation=\"required email\"\n              placeholder=\"Seu email*\"\n              onChange={changeHandler}\n              value={form.email}\n              hasError={formError}\n            />\n\n            <InputCheck\n              className=\"my-4 block md:absolute md:bottom-[-45px] z-[2]\"\n              dataValidation=\"checkbox\"\n              id=\"terms\"\n              label={terms}\n              name=\"terms\"\n              value={form.terms}\n              checked={form.terms}\n              hasError={formError}\n              onClick={(value: boolean) => {\n                setForm({ ...form, terms: value })\n                setFormError({\n                  ...formError,\n                  terms: false,\n                })\n              }}\n            />\n\n            <button\n              type=\"submit\"\n              className={cn(\n                'newsletter__btn relative w-full outline-0 min-h-11 flex items-center justify-center shadow-sm font-semibold uppercase py-3 px-4 text-sm bg-danger rounded md:min-h-[43px] md:w-auto'