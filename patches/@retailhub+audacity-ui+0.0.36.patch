  function handleToggle() {\n    setActive(!active)\n  }\n\n  function getMenuLink(menuItem: any) {\n    if (menuItem.children?.length) {\n      return null\n    }\n\n    return menuItem.link.url\n  }\n\n  return (\n    <li\n      className={cn('header-nav__item overflow-hidden', {\n        'header-nav__item--sub-hasitems':\n          data.children && data.children.length >= 1,\n      })}\n    >\n      <Button\n        className={cn('header-nav__btn relative w-full text-left', {\n          'before:opacity-0': active,\n          \"before:content-[''] before:block before:w-[15px] before:h-[1px] before:absolute before:right-2.5 before:top-1/2 before:-rotate-90 before:transition-all before:duration-300 before:linear before:bg-[color:var(--dark)] after:block after:w-[15px] after:h-[1px] after:absolute after:right-2.5 after:top-1/2 after:transition-all after:duration-300 after:linear after:bg-[color:var(--dark)]\":\n            data.children?.length,\n          'font-medium after:h-0.5': active,\n        })}\n        href={getMenuLink(data)}\n        onClick={handleToggle}\n        PlatformComponents={PlatformComponents}\n      >\n        <span>{data.link.title}</span>\n      </Button>\n      {data.children?.length ? (\n        <ThirdLevel data={data.children} active={active} />\n      ) : null}\n    </li>\n  )\n}\n\nexport default SecondLevel\n","import React, { useContext, useState } from 'react'\nimport { HeaderContext } from '../..'\n\nimport Button from '../../../button'\n// import { SubFirstLevelProps } from './models'\nimport SecondLevel from './second-level'\nimport cn from 'classnames'\nimport { HeaderNavItem } from '../types'\n\nconst MobileMenuItem: React.FC<HeaderNavItem> = ({\n  data,\n  index,\n  indexLastChild,\n}) => {\n  const {\n    toggleNav,\n    PlatformComponents,\n    controls: {\n      general: {\n        effects: { hoverBold, hoverOpacity },\n      },\n    },\n  } = useContext(HeaderContext)\n  const [activeSubContent, setActiveSubContent] = useState(false)\n  const [hover, setHover] = useState(false)\n\n  function toggleContent(value: boolean) {\n    if (data?.type === 'firstLevel') {\n      toggleNav()\n      return\n    }\n\n    setActiveSubContent(value)\n    setHover(value)\n  }\n\n  return (\n    <li\n      className={cn('block text-sm mb-5 text-[color:var(--dark)] md:hidden', {\n        'mb-0': index === indexLastChild,\n      })}\n    >\n      <Button\n        className={cn('header-nav__btn header-nav__btn--main duration-300', {\n          'font-semibold': hover && hoverBold,\n          'opacity-60': hover && hoverOpacity,\n        })}\n        href={data.type === 'firstLevel' ? data.link?.url : null}\n        onClick={() => toggleContent(true)}\n        PlatformComponents={PlatformComponents}\n        target={data.link.target}\n      >\n        {data.link.title}\n      </Button>\n      {data?.children?.length >= 1 && (\n        <div\n          className={cn(\n            'header-nav__sub fixed w-full top-[53px] left-0 h-full z-[2] bg-white pt-14 px-4 transition-all duration-300 ease-in-out',\n            {\n              hidden: !activeSubContent,\n              'block animate-[fade-effect_300ms_ease-in-out]': activeSubContent,\n            }\n          )}\n        >\n          <div className=\"header-nav__header px-4 absolute top-0 left-0 w-full h-[38px] flex items-center uppercase bg-[color:var(--primary)] text-white\">\n            <Button\n              className=\"header-nav-back__btn flex items-center justify-center w-[26px] h-[26px] bg-white rounded-[3px] before:content-[''] before:block before:w-2 before:h-2 before:border before:border-[color:var(--primary)] before:border-t-0 before:border-r-0 before:relative before:left-0.5 before:rotate-45\"\n              onClick={(_e: any) => toggleContent(false)}\n            />\n            <Button\n              href={data.link.url}\n              className=\"ml-2.5 hover:underline text-sm\"\n              onClick={(_e: any) => toggleContent(false)}\n