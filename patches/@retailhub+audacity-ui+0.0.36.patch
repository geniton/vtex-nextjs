+{"version":3,"file":"audacity-ui.cjs.development.js","sources":["../src/components/accordion/item/index.tsx","../src/components/accordion/index.tsx","../src/components/button/index.tsx","../src/helpers/create-effect-classes.ts","../src/helpers/create-slider-settings.ts","../src/components/carousel/index.tsx","../src/components/container/index.tsx","../src/components/banner/index.tsx","../src/components/headerbar-mini/index.tsx","../src/components/lazy-image/index.tsx","../src/components/bannergrid/index.tsx","../src/components/countdown/index.tsx","../src/utils/formats.ts","../src/components/stores-index/index.tsx","../src/components/footer/index.tsx","../src/components/header/profile/index.tsx","../src/components/header/burger/index.tsx","../src/components/header/wishlist/index.tsx","../src/components/header/nav/mobile-menu-item/third-level/index.tsx","../src/components/header/nav/mobile-menu-item/second-level/index.tsx","../src/components/header/nav/mobile-menu-item/index.tsx","../src/components/header/nav/desk-menu-item/second-level/index.tsx","../src/components/header/nav/desk-menu-item/third-level/sub-item/index.tsx","../src/components/header/nav/desk-menu-item/third-level/index.tsx","../src/components/header/nav/desk-menu-item/third-level-grid/index.tsx","../src/components/header/nav/desk-menu-item/index.tsx","../src/components/header/search-bar/index.tsx","../src/components/header/nav/vertical-nav-button/index.tsx","../src/components/header/nav/index.tsx","../src/components/header/cart/index.tsx","../src/components/header/account/index.tsx","../src/components/header/bottom-mobile-nav/index.tsx","../src/components/header/vertical-nav/first-level/index.tsx","../src/components/header/vertical-nav/index.tsx","../src/components/header/mask/index.tsx","../src/components/header/index.tsx","../src/components/input-text/index.tsx","../src/components/input-check/index.tsx","../src/components/modal/index.tsx","../src/utils/validations.ts","../src/components/form/index.tsx","../src/utils/api.ts","../src/utils/vtex/services/api.ts","../src/helpers/show-message-modal.ts","../src/components/newsletter/index.tsx","../src/components/loading-bricks/index.tsx","../src/components/stores/index.tsx","../src/components/tipbar/index.tsx","../src/components/product-details/index.tsx","../src/components/product-shelf/index.tsx","../src/components/cross-selling-shelf/index.tsx","../src/components/product-gallery/index.tsx","../src/components/mosaic-banner/index.tsx","../src/components/wishlist/index.tsx","../src/components/index.ts"],"sourcesContent":["import React, { FC, useState, useRef, useEffect } from 'react'\nimport cn from 'classnames'\nimport { AccordionItemProps } from '../types'\n\nconst AccordionItem: FC<AccordionItemProps> = ({\n  activeIndex,\n  customIconClass,\n  content,\n  index,\n  onActiveIndex,\n  showIcon,\n  title,\n}) => {\n  const [active, setActive] = useState(false)\n  const contentRef = useRef(null) as React.RefObject<HTMLDivElement>\n\n  function toggleAccordion() {\n    setActive(!active)\n    onActiveIndex && index ? onActiveIndex(index) : null\n  }\n\n  useEffect(() => {\n    if (activeIndex !== index) {\n      setActive(false)\n    }\n  }, [activeIndex])\n\n  const iconClass = `after:content-[\"\"] after:block after:absolute after:top-0 after:w-[2px]\n    after:h-full after:bg-black after:rounded-[20px] before:content-[\"\"] before:block\n    before:absolute before:top-0 before:w-[2px] before:h-full before:bg-black before:rounded-[20px]\n    before:rotate-90 after:transition-all after:duration-300`\n\n  return (\n    <div className=\"accordion__item\">\n      <button\n        className={cn(\n          'accordion__btn w-full text-left border-b flex items-center px-3 py-3 pr-14 relative break-words',\n          {\n            active: active,\n          }\n        )}\n        onClick={toggleAccordion}\n      >\n        {title}\n        {typeof showIcon !== 'boolean' || showIcon ? (\n          <span\n            className={cn(\n              'accordion__btn-icon absolute right-5 w-3 h-3 flex items-center justify-center',\n              iconClass,\n              customIconClass,\n              { ['after:opacity-0']: active }\n            )}\n          />\n        ) : null}\n      </button>\n      <div\n        className={cn(\n          'accordion__content transition-all duration-300 text-sm px-3 overflow-hidden',\n          {\n            'py-5': active,\n          }\n        )}\n        dangerouslySetInnerHTML={{ __html: content }}\n        ref={contentRef}\n        style={{\n          maxHeight:\n            active && contentRef.current\n              ? `${contentRef.current?.scrollHeight + 30}px`\n              : '0px',\n        }}\n      ></div>\n    </div>\n  )\n}\n\nexport default AccordionItem\n","import React, { FC, useState } from 'react'\nimport cn from 'classnames'\n\nimport AccordionItem from './item'\nimport type { AccordionProps, AccordionItemProps } from './types'\n\n// IT'S VERY IMPORTANT\n// without it the app won't render the components configs properly\n// export app configs json file & mock data\nimport AppConfigs from './app.configs.json'\nimport MockData from '../../__mocks__/accordion.json'\nexport { AppConfigs, MockData }\n\nconst Accordion: FC<AccordionProps> = ({\n  content: { items },\n  controls: {\n    general: { showIcon },\n    style: { customClass, customIconClass },\n  },\n}) => {\n  const [activeIndex, setActiveIndex] = useState(-1)\n\n  if (!items?.length) return null\n\n  return (\n    <div className={cn('accordion', customClass)}>\n      <div className=\"accordion__items\">\n        {items.map((item: AccordionItemProps, index: number) => (\n          <AccordionItem\n            activeIndex={activeIndex}\n            content={item.content}\n            customIconClass={customIconClass}\n            index={index}\n            title={item.title}\n            onActiveIndex={setActiveIndex}\n            showIcon={showIcon}\n            key={index}\n          />\n        ))}\n      </div>\n    </div>\n  )\n}\n\nexport default Accordion\n","/* eslint-disable @typescript-eslint/no-explicit-any */\nimport React from 'react'\n\ntype Props = {\n  className?: string\n  href?: string\n  children?: any\n  PlatformComponents?: any\n  [key: string]: any\n}\n\nconst Button: React.FC<Props> = ({\n  className,\n  href,\n  children,\n  target,\n  PlatformComponents,\n  ...otherProps\n}) => {\n  if (href) {\n    if (PlatformComponents?.Link) {\n      return (\n        <PlatformComponents.Link\n          href={href}\n          className={className}\n          target={target}\n          {...otherProps}\n        >\n          {children}\n        </PlatformComponents.Link>\n      )\n    }\n\n    return (\n      <a href={href} className={className} {...otherProps}>\n        {children}\n      </a>\n    )\n  }\n\n  return (\n    <button className={className} {...otherProps}>\n      {children}\n    </button>\n  )\n}\n\nexport default Button\n","import { EffectsProps } from '../types/effects'\n\nexport default function createEffectClasses(effects: EffectsProps): any {\n  if (!effects || !Object.keys(effects).length) return ''\n\n  const removeEffects: string[] = ['exchangeImage', 'bulletInside', 'hoverArrows']\n\n  const effectClasses: string[] = []\n\n  for (const effectName in effects) {\n    if (effects[effectName] && !removeEffects.includes(effectName)) {\n      effectClasses.push(effectName)\n    }\n  }\n\n  return effectClasses.filter(String).join(' ')\n}\n","import { CarouselControlsProps } from '../types/controls'\n\ntype SliderSettings = {\n  className?: string\n  arrows?: boolean;\n  dots?: boolean;\n  infinite?: boolean;\n  autoplay?: boolean;\n  autoplaySpeed?: string | number;\n  pauseOnHover?: boolean;\n  slidesToShow?: number;\n  slidesToScroll?: number;\n  showCarousel?: boolean;\n  addBulletInside?: boolean;\n  fade?: boolean;\n  applyMobileCarouselControls?: boolean\n  mobileCarouselControls?: CarouselControlsProps\n  hoverArrows?: boolean\n  children: any\n  [key: string]: any\n}\n\nfunction getSlideToShowAndScroll(controls: any) {\n  let slidesToShow = +controls.slidesToShow || 1\n  let slidesToScroll = +controls.slidesToScroll || 1\n\n  if (slidesToScroll > slidesToShow) {\n    while (slidesToScroll > slidesToShow) {\n      --slidesToScroll\n    }\n  }\n\n  return {\n    slidesToShow,\n    slidesToScroll,\n  }\n}\n\nexport default function createSliderSettings({\n  arrows = false,\n  dots = false,\n  infinite = false,\n  autoplay = false,\n  autoplaySpeed = 8,\n  speed = 0.5,\n  pauseOnHover = false,\n  slidesToShow = 1,\n  slidesToScroll = 1,\n  fade = false,\n  applyMobileCarouselControls,\n  mobileCarouselControls,\n  className,\n  addBulletInside,\n  hoverArrows,\n  children,\n  ...otherProps\n}: SliderSettings): any {\n  const dataLength: number = children ? Array.isArray(children) ? children.length : 1 : 0\n\n  const slideToShowAndScroll = getSlideToShowAndScroll({ slidesToShow, slidesToScroll })\n\n  if (!dataLength) return\n\n  function mobileResponsive() {\n    const slideToShowAndScrollMobile = getSlideToShowAndScroll(mobileCarouselControls)\n    return [\n      {\n        breakpoint: 768,\n        settings: {\n          ...mobileCarouselControls,\n          autoplaySpeed: mobileCarouselControls?.autoplaySpeed\n            ? +mobileCarouselControls?.autoplaySpeed * 1000\n            : 8000,\n            speed: mobileCarouselControls?.speed\n            ? +mobileCarouselControls?.speed * 1000\n            : 500,\n          infinite:\n            mobileCarouselControls?.infinite && dataLength > slideToShowAndScrollMobile.slidesToShow,\n          ...slideToShowAndScrollMobile,\n        },\n      },\n    ]\n  }\n\n  function deskResponsive() {\n    return [\n      {\n        breakpoint: 768,\n        settings: {\n          infinite: infinite && dataLength > 2,\n          slidesToShow: 1,\n          slidesToScroll: 1,\n        },\n      },\n    ]\n  }\n\n  const classes: any = []\n\n  if (className) {\n    classes.push(className)\n  }\n\n  if (hoverArrows) {\n    classes.push('hover-arrows')\n  }\n\n  if (addBulletInside) {\n    classes.push('bullet-inside')\n  }\n\n  return {\n    ...slideToShowAndScroll,\n    arrows,\n    dots,\n    autoplay,\n    pauseOnHover,\n    fade,\n    infinite:\n      slidesToShow && infinite\n        ? children?.length > slidesToShow\n        : infinite,\n    autoplaySpeed: autoplaySpeed\n      ? +autoplaySpeed * 1000\n      : 8000,\n    speed: speed\n      ? +speed * 1000\n      : 1000,\n    lazyLoad: 'ondemand',\n    className: classes.join(' '),\n    responsive: applyMobileCarouselControls ? mobileResponsive() : deskResponsive(),\n    ...otherProps,\n  }\n}\n","/* eslint-disable @typescript-eslint/no-explicit-any */\nimport type { ReactNode } from 'react'\nimport React from 'react'\nimport type { Settings } from 'react-slick'\nimport Slider from 'react-slick'\n\nimport createSliderSettings from '../../helpers/create-slider-settings'\n\ntype Props = {\n  children: ReactNode | ReactNode[]\n  [key: string]: any\n}\n\nconst Carousel: React.FC<Props> = ({ children, ...otherProps }) => {\n  const slickSettings: Settings = createSliderSettings({\n    children,\n    ...otherProps,\n  })\n\n  return <Slider {...slickSettings}>{children}</Slider>\n}\n\nexport default Carousel\n","import classNames from 'classnames'\nimport React from 'react'\n\nexport interface ContainerProps {\n  className?: string\n  children: React.ReactNode\n}\n\nconst Container: React.FC<ContainerProps> = ({ className, children }) => {\n  return <div className={classNames('container', className)}>{children}</div>\n}\n\nexport default Container\n","import React, { useCallback } from 'react'\nimport cn from 'classnames'\n\nimport { BannerProps, BannerPropsItem } from './types'\nimport Button from '../button'\nimport createEffectClasses from '../../helpers/create-effect-classes'\nimport Carousel from '../carousel'\nimport Container from '../container'\n\n// IT'S VERY IMPORTANT\n// without it the app won't render the components configs properly\n// export app configs json file\nimport AppConfigs from './app.configs.json'\nimport MockData from '../../__mocks__/banner.json'\nexport { AppConfigs, MockData }\n\nconst Banner: React.FC<BannerProps> = ({\n  content: { items },\n  controls: {\n    general: {\n      applyMobileCarouselControls,\n      carouselControls,\n      deskVariations,\n      effects: { hoverArrows, ...effectsProps },\n      mobileCarouselControls,\n      mobileVariations,\n    },\n    style: { backgroundColor, customClass, color },\n  },\n  PlatformComponents,\n}) => {\n  const renderImages = ({ mobileImage, deskImage }: BannerPropsItem) => {\n    if (!mobileImage && !deskImage) return null\n\n    const { url, width, height, alt } = mobileImage || deskImage\n\n    return (\n      <picture>\n        <source srcSet={deskImage?.url} media=\"(min-width: 1026px)\" />\n        {\n          <PlatformComponents.Image\n            image={url}\n            width={width}\n            height={height}\n            alt={alt}\n            preload\n            loading=\"eager\"\n            fetchPriority=\"high\"\n          />\n        }\n      </picture>\n    )\n  }\n\n  const renderBannerItem = useCallback(\n    (item: BannerPropsItem) => {\n      const bannerItemClasses = cn(\n        'banner__item',\n        createEffectClasses(effectsProps)\n      )\n\n      const videoContent = (\n        <>\n          <video autoPlay loop playsInline muted className=\"lg:hidden\">\n            <source\n              src={item.mobileVideo?.url || item.deskVideo?.url}\n              type=\"video/mp4\"\n            />\n          </video>\n          <video autoPlay loop playsInline muted className=\"hidden lg:block\">\n            <source src={item.deskVideo?.url} type=\"video/mp4\" />\n          </video>\n        </>\n      )\n\n      if (item.isVideo) {\n        if (item.link) {\n          return (\n            <Button\n              href={item.link.url}\n              target={item.link.target}\n              PlatformComponents={PlatformComponents}\n              className={bannerItemClasses}\n            >\n              {videoContent}\n            </Button>\n          )\n        }\n\n        return videoContent\n      }\n\n      return (\n        <article className={bannerItemClasses} id={item.id}>\n          {item.link?.url ? (\n            <figure className=\"banner__figure\">\n              <Button\n                href={item.link.url}\n                target={item.link.target}\n                PlatformComponents={PlatformComponents}\n              >\n                {renderImages(item)}\n              </Button>\n            </figure>\n          ) : (\n            <figure className=\"banner__figure\">{renderImages(item)}</figure>\n          )}\n\n          {item.showDesc && (\n            <div\n              className=\"banner__content rich-text p-6 bg-light text-center\"\n              style={{\n                backgroundColor,\n                color,\n              }}\n              dangerouslySetInnerHTML={{ __html: item.desc }}\n            />\n          )}\n        </article>\n      )\n    },\n    [effectsProps, backgroundColor, color]\n  )\n\n  if (!items?.length) {\n    return null\n  }\n\n  return (\n    <div\n      className={cn('banner', customClass, {\n        'mobile-only:hidden': mobileVariations.hide,\n        'lg:hidden': deskVariations.hide,\n      })}\n    >\n      <Container\n        className={cn({\n          'mobile-only:p-0': mobileVariations.full,\n          'md:p-0': deskVariations.full,\n          effect: hoverArrows,\n        })}\n      >\n        <div className=\"banner__items\">\n          <Carousel\n            {...carouselControls}\n            applyMobileCarouselControls={applyMobileCarouselControls}\n            mobileCarouselControls={mobileCarouselControls}\n            hoverArrows={hoverArrows}\n          >\n            {items.map((item: BannerPropsItem, index: number) => (\n              <div key={index}>{renderBannerItem(item)}</div>\n            ))}\n          </Carousel>\n        </div>\n      </Container>\n    </div>\n  )\n}\n\nexport default Banner\n","import React from 'react'\n\nimport Button from '../button'\nimport { HeaderbarMiniProps } from './types'\n\nconst HeaderbarMini: React.FC<HeaderbarMiniProps> = ({\n  color,\n  link,\n  PlatformComponents,\n  sidebarText,\n  title,\n}) => {\n  function headerAside() {\n    if (link?.url) {\n      return (\n        <Button\n          className=\"headerbar-mini__btn absolute right-0 md:right-[40px] text-xs\"\n          href={link.url}\n          PlatformComponents={PlatformComponents}\n        >\n          {link.title}\n        </Button>\n      )\n    }\n\n    return (\n      sidebarText && (\n        <p\n          className=\"headerbar-mini__text text-xs\"\n          dangerouslySetInnerHTML={{ __html: sidebarText }}\n        />\n      )\n    )\n  }\n\n  return title ? (\n    <header\n      className=\"headerbar-mini container mb-8 relative flex items-center\"\n      style={{\n        color,\n      }}\n    >\n      {title && (\n        <div\n          className=\"headerbar-mini__title block w-full\"\n          dangerouslySetInnerHTML={{ __html: title }}\n        />\n      )}\n      {headerAside()}\n    </header>\n  ) : null\n}\n\nexport default HeaderbarMini\n","import React, { useEffect, useState, useRef } from 'react'\n\nimport cn from 'classnames'\nimport { LazyImageProps } from './types'\n\nconst LazyImage: React.FC<LazyImageProps> = ({\n  className,\n  image,\n  src = '',\n  style = {},\n  ...props\n}) => {\n  const [imageVisible, setVisibleImage] = useState(false)\n  const [loaded, setLoaded] = useState(false)\n  const [customProps, setCustomProps] = useState({})\n  const [url, setUrl] = useState(\n    'data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAAAXNSR0IArs4c6QAAADhlWElmTU0AKgAAAAgAAYdpAAQAAAABAAAAGgAAAAAAAqACAAQAAAABAAAAAaADAAQAAAABAAAAAQAAAADa6r/EAAAAC0lEQVQIHWP4DwQACfsD/Qy7W+cAAAAASUVORK5CYII='\n  )\n  const imageRef = useRef<any>(null)\n\n  const handleScroll = () => {\n    const top = imageRef.current?.getBoundingClientRect().top\n    if (parseInt(top, 10) < window.innerHeight + 300) {\n      setVisibleImage(true)\n      setUrl(typeof image === 'string' ? image : image?.url || src)\n      setCustomProps({\n        alt: props?.alt || image?.alt,\n        width: props?.width || image?.width,\n        height: props?.height || image?.height,\n        style: {\n          maxHeight: props?.maxHeight ? `${props?.maxHeight}px` : '',\n          ...style,\n        },\n      })\n      window.removeEventListener('scroll', handleScroll, true)\n    } else {\n      setVisibleImage(false)\n    }\n  }\n\n  useEffect(() => {\n    if (!imageRef.current) return\n\n    handleScroll()\n\n    if (window.addEventListener) {\n      window.addEventListener('scroll', handleScroll, true)\n    } else {\n      window.attachEvent('onscroll', handleScroll)\n    }\n\n    return () => {\n      window.removeEventListener('scroll', handleScroll, true)\n    }\n  }, [image, src])\n\n  return (\n    <img\n      className={cn(className, 'opactiy-0', {\n        'opacity-100': imageVisible && loaded,\n      })}\n      onLoad={() => setLoaded(true)}\n      ref={imageRef}\n      src={url}\n      {...customProps}\n    />\n  )\n}\n\nexport default LazyImage\n","/* eslint-disable radix */\nimport React, { PropsWithChildren } from 'react'\nimport cn from 'classnames'\n\nimport HeaderbarMini from '../headerbar-mini'\nimport Button from '../button'\nimport LazyImage from '../lazy-image'\nimport type {\n  BannergridProps,\n  BannergridPropsItem,\n  BannergridFeaturedsItem,\n} from './models'\nimport createEffectClasses from '../../helpers/create-effect-classes'\nimport Carousel from '../carousel'\n\n// IT'S VERY IMPORTANT\n// without it the app won't render the components configs properly\n// export app configs json file